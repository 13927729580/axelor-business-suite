<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_3.0.xsd">
	
	<grid name="subscription-grid" title="Subscriptions" model="com.axelor.apps.supplychain.db.Subscription">
		<field name="invoicingDate"/>
	    <field name="fromPeriodDate"/>
	    <field name="toPeriodDate"/>
	    <field name="quantity"/>
	    <field name="amount"/>
	    <field name="invoiced"/>
	    <button name="invoiceSubscription" onClick="com.axelor.apps.supplychain.web.SaleOrderInvoiceController:generateInvoice"/>
	</grid>
	
</object-views>