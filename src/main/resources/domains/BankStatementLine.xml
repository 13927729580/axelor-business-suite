<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
  
  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="BankStatementLine" lang="java">

	<string name="name" title="Label"/>
    <many-to-one name="bankStatement" ref="com.axelor.apps.account.db.BankStatement" title="Bank Statement" />
    <date name="effectDate" title="Effect Date" />
    <string name="reference" title="Reference"/>
    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Partner" />
    <integer name="typeSelect" title="Type" selection="account.bank.statement.type.select"/>
    <many-to-one name="account" ref="com.axelor.apps.account.db.Account" title="Account" />
    <decimal name="amount" title="Amount" /> 
    <many-to-one name="moves" ref="com.axelor.apps.account.db.Move" title="Moves" />
    
  </entity>
  
</domain-models>
