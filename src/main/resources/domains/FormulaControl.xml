<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
  <module name="base" package="com.axelor.apps.base.db"/>
  
  <entity name="FormulaControl" lang="java">
    
    <many-to-one name="tax" ref="com.axelor.apps.account.db.Tax" title="Tax"/>
    <many-to-one name="taxLine" ref="com.axelor.apps.account.db.TaxLine" title="Tax line" />
    <many-to-one name="calculationRule" ref="com.axelor.apps.account.db.CalculationRule" title="Tax Calculation rule"/>
    <many-to-one name="vatManagement" ref="com.axelor.apps.account.db.VatManagement" title="VAT Configuration" />
    
    <string name="control" title="Control" required="true"/>
    <string name="msg" title="Message" namecolumn="true"/>
 
  </entity>
  
</domain-models>
