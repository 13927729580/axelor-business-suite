<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="organisation" package="com.axelor.apps.organisation.db"/>

  <entity name="TaskUpdateLine" lang="java">
  	
  	<many-to-one name="task" ref="com.axelor.apps.organisation.db.Task" title="Task"/>
  	<date name="date" title="Date" readonly="true"/>
  	<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" title="User"/>
  	<decimal name="revenue" title="Revenue"/>
  	<decimal name="cost" title="Cost"/>
  	
  </entity>

</domain-models>
