<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ProdResource" lang="java">
  
    <string name="label" namecolumn="true" title="Label"/>
    <many-to-one name="machine" ref="com.axelor.apps.production.db.Machine" title="Machine"/>
    <many-to-one name="planning" ref="com.axelor.apps.organisation.db.Planning" title="Planning"/>
    <one-to-many name="prodResouceLineList" ref="com.axelor.apps.production.db.ProdResourceLine" mappedBy="prodResource" title="Human resources"/>
    
  </entity>
</domain-models>
