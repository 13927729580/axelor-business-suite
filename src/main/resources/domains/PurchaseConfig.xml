<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="purchase" package="com.axelor.apps.purchase.db"/>

  <entity name="PurchaseConfig" lang="java" cachable="true">
  	<integer name="importId"/>
    <string name="name" title="Name" required="true"/>
    <string name="code" title="Code" unique="true"/>
    
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company" unique="true"/>

  	<integer name="purchaseOrderInvoicingTypeSelect" title="Purchases order invoicing type" selection="purchase.purchase.order.invoicing.type.select"/>
  	
  </entity>
  
</domain-models>
