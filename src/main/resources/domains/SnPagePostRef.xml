<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="socialnetwork" package="com.axelor.apps.socialnetwork.db"/>

  <entity name="SnPagePostRef">
  	<![CDATA[
    This class indicates for a Facebook status the page on which it has been posted 
    and the id of the status on this page
 	For a status update on multiple pages, there will be a list of this entity
    @author David Nguyen
    ]]>
  	<string name="messageUpdateRef"/>
  	<many-to-one name="snStatusUpdates" ref="com.axelor.apps.socialnetwork.db.SnStatusUpdates"/>
	<many-to-one name="snPage" ref="com.axelor.apps.socialnetwork.db.SnPage"/>
  </entity>
 
</domain-models>