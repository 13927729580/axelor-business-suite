<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="ChequeRejection" lang="java">
  	<string name="name" title="Reference" readonly="true"/>
   	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company" required="true"/>
    <datetime name="dateTime" title="Date" tz="true" readonly="true"/>
    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Customer"/>
  	<many-to-one name="paymentVoucher" ref="com.axelor.apps.account.db.PaymentVoucher" title="Payment voucher" required="true"/>
  	<decimal name="amountRejected" title="Rejected amount" readonly="true"/>
  	<date name="rejectionDate" title="Reject Date" required="true"/>
    <many-to-one name="interbankCodeLine" ref="com.axelor.apps.account.db.InterbankCodeLine" title="Rejection reason" required="true"/>
  	<many-to-one name="status" ref="com.axelor.apps.base.db.Status" title="Status" readonly="true"/>
  	<many-to-one name="move" ref="com.axelor.apps.account.db.Move" title="Rejection MoveLine"/>
  	<many-to-one name="paymentMode" ref="com.axelor.apps.account.db.PaymentMode" title="Payment Mode" hidden="true"/>
    <string title="Description" name="description" default="Cheque rejection"/>
  </entity>

</domain-models>