<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="SocialNetUserAccount">
		<many-to-one name="socialNet" ref="com.axelor.apps.socialnetwork.db.SocialNet" title="Social Network Type"/>
		<string name="snUserRef" title="Social Network User ID"/>
		<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" orphan="true" />
		<string name="name" title="User Name"/>
		<string name="link" title="Link to profile website"/>
	</entity>
</domain-models>