<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
  
  <module name="base" package="com.axelor.apps.base.db"/>
  
  <entity name="Import" lang="java">
  
    <string name="typeSelect" title="Import type" default="csv" required="true" selection="iimport.type.select"/>
    <string name="name" title="Name"/>
    <string name="path" title="Folder path containing files to import" max="2048"/>
    <string name="configPath" title="Path to XML import mapping file" help="true" />
    <string name="log" title="Log" multiline="true"  large="true"/>
  
  </entity>

</domain-models>
