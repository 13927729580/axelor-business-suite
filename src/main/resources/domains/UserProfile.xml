<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">
	<module name="googleapps" package="com.axelor.apps.googleapps.db" />
	<entity name="UserProfile" sequential="true" >
		<many-to-one name="driveUser" ref="com.axelor.auth.db.User" title="Drive User" orphan="true"  ></many-to-one>
		<string name="emailId" title="Email Id" />
		<string name="name" title="Name" namecolumn="true" />		
		<string name="permissionId" title="Account Id" hidden="true" help="Google Account Id/Permission Id to share File"></string>
	</entity>
</domain-models>
