<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="StockMove" lang="java">
  
	<integer name="importId" />

	<string name="stockMoveSeq" namecolumn="true" title="Reference" readonly="true"/>
    <string name="name" title="Name" required="true"/>
    <many-to-one name="fromLocation" ref="com.axelor.apps.supplychain.db.Location" title="From location"/>
    <many-to-one name="toLocation" ref="com.axelor.apps.supplychain.db.Location" title="To location"/>
    
    <date name="estimatedDate" title="Estimated date"/>
    <date name="realDate" title="Move date" readonly="true"/>
    
    <integer name="statusSelect" title="Status" selection="supplychain.stock.move.status.select" readonly="true"/>
    <integer name="typeSelect" title="Type Status" selection="supplychain.stock.move.type.select"/>
    
    <many-to-one name="fromAddress" ref="com.axelor.apps.base.db.Address" title="From address"/>
    <many-to-one name="toAddress" ref="com.axelor.apps.base.db.Address" title="To address"/>
    
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Partner"/>
    
    <one-to-many name="stockMoveLineList" ref="com.axelor.apps.supplychain.db.StockMoveLine" mappedBy="stockMove" title="Stock move content"/>
    
    <!-- Onglet Configuration -->
    <many-to-one name="salesOrder" ref="com.axelor.apps.supplychain.db.SalesOrder" title="Sale order"/>
    <many-to-one name="purchaseOrder" ref="com.axelor.apps.supplychain.db.PurchaseOrder" title="Purchase order"/>
    <many-to-one name="invoice" ref="com.axelor.apps.account.db.Invoice" title="Invoice"/>
    
  </entity>
</domain-models>
