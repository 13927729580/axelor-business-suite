<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="ApplicationCredentials">
		<![CDATA[
	    This class is used to store the application keys.
	    Those keys are necessary to authorize the API and get access to the methods.
	    redirectUrl is the link where the user will be sent after the authorization process has succeed
	    @author David Nguyen
	    ]]>
		<string name="apikey" title="API Key" required="true"
			help="API Application's Key"/>
		<string name="apisecret" title="API Secret" required="true"
			help="API Application's Secret Key"/>
		<many-to-one name="socialNet" ref="com.axelor.apps.socialnetwork.db.SocialNet" title="Network Type"
			unique="true" help="Social Networking Type"/>
		<string name="redirectUrl" title="Redirect URL" required="true"
			help="You must Specify Same URL which you specify under Social Networking API Applicaton's ,eg:- FOR Facebook WEBSITE WITH FACEBOOK LOGIN"/>
	</entity>
</domain-models>