<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="base" package="com.axelor.apps.base.db" />
	<entity name="ApplicationCredentials">
		<string name="apikey" title="API Key" required="true"
			help="API Application's Key"></string>
		<string name="apisecret" title="API Secret" required="true"
			help="API Application's Secret Key"></string>
		<!-- <many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" title="Current 
			User" help="Currently Active ERP User"></many-to-one> -->
		<many-to-one name="socialNet" ref="com.axelor.apps.base.db.SocialNet" title="Network Type"
			unique="true" help="Social Networking Type"></many-to-one>
		<string name="redirectUrl" title="Redirect URL" required="true"
			help="You must Specify Same URL which you specify under Social Networking API Applicaton's ,eg:- FOR Facebook WEBSITE WITH FACEBOOK LOGIN"></string>
	</entity>
</domain-models>