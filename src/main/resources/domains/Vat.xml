<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="Vat" lang="java" cachable="true">
  
    <integer name="importId"/>
    <string name="name" title="Name" required="true"/>
    <string name="code" title="Code" required="true" unique="true"/>
    <many-to-one name="activeVatLine" ref="com.axelor.apps.account.db.VatLine" title="V.A.T. active version"/>
    
    <one-to-many name="vatLineList" ref="com.axelor.apps.account.db.VatLine" mappedBy="vat" title="VAT versions"/>
    
  	<one-to-many name="vatAccountList" ref="com.axelor.apps.account.db.VatAccount" mappedBy="vat" title="VAT account configuration"/>
  
 	<unique-constraint columns="code,name"/>
  
  </entity>

</domain-models>