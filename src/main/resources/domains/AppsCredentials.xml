<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">

	<module name="googleapps" package="com.axelor.apps.googleapps.db" />
	<entity name="AppsCredentials" sequential="true" >
			<many-to-one name="driveUser" ref="com.axelor.auth.db.User" title="Drive User"  help="Google Apps User "></many-to-one>	
				
			<string name="accessToken" required="false" title="Access Token" help="true" />
			<string name="refreshToken" required="false"  title="Refresh Token" help="true" />
			<string name="lastAccessTime" required="false" title="Last Access Time" help="true"></string>
			<many-to-many name="googleServiceScope" ref="com.axelor.apps.googleapps.db.GoogleServiceScope" required="false" title="Google Scope" />
	</entity>

</domain-models>
