<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
	<module name="consulting" package="com.axelor.apps.consulting.db" />
			
		<entity name="PriceListLine"  lang="java">
			<integer name="minExperience" title="Minimum experience"/>
			<integer name="maxExperience" title="Maximum experience"/>
			<decimal name="price" title="Prix" precision="20" scale="5"/>
			<many-to-one name="product" ref="com.axelor.apps.base.db.Product"/>
			<many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit"/>
			<many-to-one name="priceList" ref="PriceList" title="Price List"/>
		</entity>
		
</domain-models>