<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
	<module name="base" package="com.axelor.apps.base.db" />
			
		<entity name="PriceListLine"  lang="java">
			<many-to-one name="priceList" ref="com.axelor.apps.base.db.PriceList" title="Price list"/>
			<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product"/>
			<many-to-one name="productCategory" ref="com.axelor.apps.base.db.ProductCategory" title="Product category"/>
			<integer name="typeSelect" title="Discount/Additionnal/Replace" selection="base.price.list.line.type.select"/>
			<integer name="amountTypeSelect" title="Fixe/%" selection="base.price.list.line.amount.type.select"/>
			<decimal name="amount" title="Amount/%"/>
			<decimal name="minQty" title="Quantity min (by product)"/>
		</entity>
		
</domain-models>