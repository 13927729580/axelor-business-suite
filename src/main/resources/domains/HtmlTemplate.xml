<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="HtmlTemplate" lang="java">
  	
  	<string name="name" title="Name"/>
  	<string name="code" title="Code" unique="true"/>
<!--   	<integer name="typeSelect" title="Type" selection="base.html.template.type.select"/> -->
  	<many-to-one name="metaModel" ref="com.axelor.meta.db.MetaModel" title="Object concerned"/>
    <string name="content" title="Content" large="true"/>
    <one-to-many name="trackerList" ref="com.axelor.apps.marketing.db.Tracker" title="Trackers"/>
    
  </entity>

</domain-models>
