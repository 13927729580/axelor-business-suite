<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="PricingList" lang="java" cachable="true">
  
    <string title="Nom" name="name"/>
    <many-to-one ref="KeyPricingList" title="Clé du barème" name="keyPricingList"/>
    <one-to-many ref="PricingListVersion" title="Versions de barème" name="pricingListVersionList" mappedBy="pricingList" orderBy="fromDate"/>
    <integer title="Type" default="0" name="typeSelect" selection="pricing.list.type.select"/>
  
  </entity>
  
</domain-models>
