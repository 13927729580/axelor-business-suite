<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="FormulaGenerator" lang="java">
   
    <string name="name" title="Label" required="true"/>
    <datetime name="dateGeneration" title="Date" tz="true"/>
    
    <string name="formula" large="true" title="Formula"/>
    <string name="formulaLog" large="true" title="Formula log"/>
    
    <string name="displayFormula" large="true" title="Display formula"/>
    <string name="displayFormulaLog" large="true" title="Display formula log"/>
    
    <string name="qtyFormula" large="true" title="Qty coefficient formula"/>
    <string name="qtyFormulaLog" large="true" title="Qty coefficient formula log"/>
    
    <string name="conditionFormula" large="true" title="Formula condition"/>
    <string name="conditionFormulaLog" large="true" title="Formula condition log"/>
    
    <string name="calculationRuleTax" large="true" title="Tax formula"/>
    <string name="calculationRuleTaxLog" large="true" title="Tax formula Log"/>
    
    <string name="conditionCalculationRuleTax" large="true" title="Tax formula condition"/>
    <string name="conditionCalculationRuleTaxLog" large="true" title="Tax formula condition log"/>
    
    <string name="conditionTax" large="true" title="Tax condition"/>
    <string name="conditionTaxLog" large="true" title="Tax condition log"/>
    
    <string name="vatFormula" large="true" title="VAT formula"/>
    <string name="vatFormulaLog" large="true" title="VAT formula log"/>
    
  </entity>

</domain-models>
