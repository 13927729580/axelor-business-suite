<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="FBPages">
		<string name="pageId" help="Unique Facebook Page ID"/>
		<string name="name" title="Page Name" help="Facebook Registered Page Name"/>
		<string name="username" title="User Name" help="Facebook Registered Page Username"/>
		<string name="pageUrl" title="URL of Page" help="Global Link of the Facebook Page"/>
		<one-to-many name="fbPagePostList" ref="com.axelor.apps.socialnetwork.db.FBPagePost" title="Post Content" orphan="true" help="Content Posted on Page"/>		
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" help="Currently Active ERP User"/>
	</entity>
</domain-models>
