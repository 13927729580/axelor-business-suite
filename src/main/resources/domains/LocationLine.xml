<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="LocationLine" lang="java">
  
  	<many-to-one name="location" ref="com.axelor.apps.supplychain.db.Location" title="Location"/>
  	
	<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product"/>    
	<decimal name="currentQty" title="Current quantity"/>
	<decimal name="futureQty" title="Future quantity"/>
	<date name="lastFutureStockMoveDate" title="Last Future Stock Move"/>
	
	<many-to-one name="trackingNumber" ref="com.axelor.apps.base.db.TrackingNumber" title="TrackingNumber"/>
    <many-to-one name="productVariant" ref="com.axelor.apps.base.db.ProductVariant" title="Variants"/>
	<many-to-one name="detailsLocation" ref="com.axelor.apps.supplychain.db.Location" title="Location Details"/>
	
  </entity>
</domain-models>
