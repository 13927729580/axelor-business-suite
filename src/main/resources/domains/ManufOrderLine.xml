<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ManufOrderLine" lang="java">
  
  	<many-to-one name="manufOrder" ref="com.axelor.apps.production.db.ManufOrder" title="Manufacturing order"/>
  	<many-to-one name="resource" ref="com.axelor.apps.production.db.ProdResource" title="Resource"/>
  	<datetime name="plannedStartDateT" title="Planned start date"/>
  	<datetime name="plannedEndDateT" title="Planned end date"/>
  	<integer name="statusSelect" title="Status" selection="production.prod.order.status.select"/>
    
    <datetime name="realStartDateT" title="Real start date"/>
  	<datetime name="realEndDateT" title="Real end date"/>
    
  </entity>
</domain-models>
