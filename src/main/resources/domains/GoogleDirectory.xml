<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
		<module name="googleapps" package="com.axelor.apps.googleapps.db" />
		<entity name="GoogleDirectory">
			
			<string name="directoryId" title="Directory Id" help="true" />
			<string name="directoryName" title="directory Name" namecolumn="true"  help="true"/> 
			<boolean name="trashed" title="Trashed" default="false" help="true" />
			<many-to-one name="parentGoogleDirectory" ref="com.axelor.apps.googleapps.db.GoogleDirectory" title="Parent Directory" help="true" />
			<many-to-one name="driveUserInfo" ref="com.axelor.apps.base.db.UserInfo" orphan="true" help="true" />
			<one-to-many name="googleFilesList" ref="GoogleFile" mappedBy="googleDirectory" orphan="true"  help="true"/>	
				
	    </entity>
</domain-models>
