<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="marketing" package="com.axelor.apps.marketing.db"/>

  <entity name="TrackerInfo" lang="java">
  	
  	<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Contact"/>
  	<many-to-one name="lead" ref="com.axelor.apps.crm.db.Lead" title="Lead"/>
  	<string name="trackerKey" title="Tracker key"/>
  	<string name="campaignCode" title="Campaign code"/>
  	<string name="location" title="Location"/>
  	<string name="browser" title="Browser"/>
  	<string name="redirection" title="Redirection"/>
    <datetime name="hour" title="Hour"/>
    
  </entity>

</domain-models>
