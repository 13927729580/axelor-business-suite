<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="Agency" lang="java">
  
    <integer name="importId"/>
    <string name="name" title="Name"/>
    <string name="nameLong" title="Agency name long" namecolumn="true"/>
    <string name="nameShort" title="Agency name short" />
    <string name="code" title="Code"/>
    <string name="num" title="Agency NÂ°" unique="true" />
    <many-to-one name="address" ref="com.axelor.apps.base.db.Address" title="Address"/>
    <many-to-one name="parentAgency" ref="Agency" title="Parent agency"/>
    <string name="openingHours" title="Opening hours"/>
	<one-to-many name="userInfoList" ref="com.axelor.apps.base.db.UserInfo" title="Employees" mappedBy="agency"/>
 
  </entity>
  
</domain-models>
