<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />

	<entity name="SnStatusComments">
		<many-to-one name="snStatusUpdates" ref="com.axelor.apps.socialnetwork.db.SnStatusUpdates" orphan="true" title="Post"/>
		<string name="commentId" title="Comment ID" readonly="true"/>
		<string name="commentText" title="Comment" multiline="true" max="700" />
		<datetime name="commentDateT" title="Time" tz="true" readonly="true"/>
		<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" orphan="true" title="By" readonly="true"/>
		<many-to-one name="companyPartner" ref="com.axelor.apps.base.db.Partner" orphan="true" title="Page"/>
		<boolean name="isFacebook" title="Facebook ?"/>
		<boolean name="isFacebookWall" title="Comment on Wall ?"/>
		<boolean name="isLinkedin" title="LinkedIn ?"/>
		<boolean name="isTwitter" title="Twitter ?"/>
	</entity>

</domain-models>