<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="PaymentMode" lang="java" cachable="true">
  
  	<integer name="importId"/>
    <string name="name" required="true" unique="true" title="Label"/>
    <string name="code" title="Code"/>
    <one-to-many name="accountManagementList" ref="com.axelor.apps.account.db.AccountManagement" mappedBy="paymentMode" title="Accounts by paymentMode"/>
    <many-to-one name="bankJournal" ref="com.axelor.apps.account.db.Journal" title="Journal"/>
    <string name="invoiceLabel" title="Invoice label" help="Label to be displayed on invoice PDF"/>
  	<integer name="typeSelect" title="type" selection="iaccount.payment.mode.type.select"/>
  	<integer name="salesPurchasesSelect" title="Sales / Purchases ?" selection="iaccount.payment.mode.sales.purchases.selects"/>
  
  </entity>

</domain-models>
