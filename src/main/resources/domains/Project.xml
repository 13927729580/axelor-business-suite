<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="organisation" package="com.axelor.apps.organisation.db"/>

  <entity name="Project" lang="java">
  
  	<many-to-one name="clientPartner" ref="com.axelor.apps.base.db.Partner" title="Customer"/>
  	<many-to-one name="contactPartner" ref="com.axelor.apps.base.db.Partner" title="Contact"/>
  	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" readonly="true" title="Société"/>
  	<string name="affairName" title="Affair name" required="true" namecolumn="true"/>
  	<string name="affairDescription" title="Affair description"/>
  	<string name="description" large="true" multiline="true" title="Description"/>
  	<boolean name="isModel" title="Model ?"/>
  	<boolean name="isProject" title="Project ?"/>
  	<boolean name="isAffair" title="Affair ?"/>
  	<boolean name="isTimeSheetImputable" title="Timesheet input required ?"/>
  	<many-to-one name="defaultTask" ref="com.axelor.apps.organisation.db.Task" title="Default task"/>
  	<boolean name="isReinvoicingExpense" title="Reinvoicing of travel expense"/>
  	<boolean name="isReinvoicingTime" title="Reinvoicing time spent"/>
  	<integer name="projectStatusSelect" title="Status" selection="organisation.project.status.select"/>
  	<integer name="realEstimatedMethodSelect" title="Real estimated compute method" selection="organisation.project.real.estimated.method.select"/>
  	
  	<!-- Financial informations -->
  	<decimal name="initialEstimatedTurnover" title="Turnover" readonly="true"/>
  	<decimal name="initialEstimatedCost" title="Cost" readonly="true"/>
  	<decimal name="initialEstimatedMargin" title="Margin" readonly="true"/>
  	<decimal name="realEstimatedTurnover" title="Turnover" readonly="true"/>
  	<decimal name="realEstimatedCost" title="Cost" readonly="true"/>
  	<decimal name="realEstimatedMargin" title="Margin" readonly="true"/>
  	<decimal name="realInvoicedTurnover" title="Turnover" readonly="true"/>
  	<decimal name="realInvoicedCost" title="Cost" readonly="true"/>
  	<decimal name="realInvoicedMargin" title="Margin" readonly="true"/>
  	
  	<decimal name="plannedTime" title="Planned time" readonly="true"/>
  	<decimal name="spentTime" title="Spent time" readonly="true"/>
  	<decimal name="remainingTime" title="Reminaing time" readonly="true"/>
  	<decimal name="totalTime" title="Total time"/>
  	  
 	<many-to-one name="parentProject" ref="com.axelor.apps.organisation.db.Project" title="Parent project"/>
 	<many-to-one name="unit" title="Project unit" ref="com.axelor.apps.base.db.Unit" required="true"/>
  	
  	<one-to-many name="taskList" ref="com.axelor.apps.organisation.db.Task" mappedBy="project" title="Tasks"/>
	  	
  	<many-to-one name="responsibleUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Responsible"/>
  	<many-to-many name="memberUserInfoSet" ref="com.axelor.apps.base.db.UserInfo" title="Members"/>
  	
  	<many-to-one name="modelProject" ref="com.axelor.apps.organisation.db.Project" title="Project model"/>
  	<many-to-one name="preSalesTask" ref="com.axelor.apps.organisation.db.Task" title="Task"/>
  	<boolean name="imputExpense" title="Imputation note de frais"/>
  	<integer name="typeProjectSelect" title="Type select" selection="organisation.project.type.project.select"/>
  	
  	<boolean name="isAdministrative" title="Administrative ?"/>
  	
    <!-- Need -->
    <date name="consultationDate" title="Consultation Date"/>
    <string name="typeSelect" title="Type" selection="consulting.need.type.select"/>
    <date name="deadline" title="Deadline"/>
    <string name="complexity" title="Complexity" selection="grade.0.up.to.5.select"/>
	<string name="competition" title="Competition" selection="grade.0.up.to.5.select"/>
    <decimal name="plannedBudget" title="Planned budget"/>
    <string name="needDescription" title="Need Description" large="true"/>
    <binary name="specificationFile" title="Attach specification"/> 
    <datetime name="answerDate" title="Answer Date"/>
    <many-to-many name="candidateSet" title="Candidates" ref="com.axelor.apps.organisation.db.Candidate"/>
    <many-to-many name="employeeSet" title="Employees" ref="com.axelor.apps.organisation.db.Employee"/>
    <integer name="marketTypeSelect" title="Market Type" selection="organisation.affair.market.type.select"/>
    <string name="exportTypeSelect" title="Export Type" selection="organisation.project.report.type.select"/>

<!--     TRSB -->
    <many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" title="Dossier présenté par"/>
    <string name="solutionAttendue" title="Solution attendue"/>
    <date name="comiteEngagement" title="Comité d'engagement"/>
    <integer name="statusTypeSelect" title="Statut" selection="organisation.project.status.type.select"/>
    <string name="motif" title="Motif"/>
    
    <!-- Onglet Information commerciale -->
    <many-to-one name="agency" ref="com.axelor.apps.organisation.db.Agency" title="Agence TRSB"/>
    <many-to-one name="daUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="DA"/>
    <many-to-one name="icUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="IC"/>
    <many-to-one name="source" ref="com.axelor.apps.base.db.Source" title="SOURCE"/>
    <string name="agenda" title="Agenda"/>
    <string name="critEvalOffre" title="Critères d'évaluation de l'offre"/>
    <string name="descriptif" title="Descriptif"/>
    
    <!-- Onglet Qualification avant-vente -->
    <many-to-one name="respQualifUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Resp. qualif."/>
    <integer name="engagementTypeSelect" title="Engagement" selection="organisation.project.engagement.type.select"/>
    <string name="solution" title="Solution"/>
    <string name="technologies" title="Technologies"/>
    <decimal name="macroChiffMarche" title="Macro chiffrage du Marché (€HT)"/>
    <decimal name="estimationParAnnee" title="Estimation par année"/>
    <decimal name="estimationParPhaseLot" title="Estimation par Phase, Par Lot"/>
    <date name="receptionDossierDate" title="Réception du dossier"/>
    <date name="remiseQualifDate" title="Remise de la qualification"/>
    <date name="finalOffreTechDate" title="Finalisation de l'offre technique"/>
    <date name="finalOffreComDate" title="Finalisation de l'offre commerciale"/>
    <date name="remiseOffrePrevueDate" title="Remise de l'offre prévue"/>
    <integer name="chargesQualif" title="Charges de qualification (en 1/4 de j/h)"/>
    <integer name="estimationChargesRep" title="Estimation des Charges de réponse"/>
    <string name="qualifDossier" title="Qualification du dossier"/>
    <string name="pointImportant" title="Point IMPORTANTS"/>
    <string name="analyseNote" title="Analyse : Note / 20"/>
    <integer name="conclusionAvtVenteTypeSelect" title="Conclusion avant-vente" selection="organisation.project.conclusion.avt.vente.type.select"/>
    
    <!-- Onglet Qualification technique -->
    <string name="respCdsSdm" title="RESP CDS/SDM"/>
    <string name="expert" title="EXPERT"/>
    <string name="positionnementCDS" title="Positionnement de notre CDS sur cette affaire"/>
    <string name="pointMarquantDossier" title="Point Marquant du dossier / VA-VAPA"/>
    <string name="analyseRisquesSolutions" title="Analyse des Risques / Solutions"/>
    <string name="avisDirectionTechnique" title="AVIS DE LA DIRECTION TECHNIQUE / CDS CIBLE"/>
    
    <!-- Onglet GO/NOGO -->
    <integer name="goNoGoTypeSelect" title="GO/NOGO" selection="organisation.project.go.no.go.type.select"/>
    <date name="todayDate" title="Le"/>
    <many-to-one name="dirTechniqueUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Direction Technique"/>
    <many-to-one name="dirCommercialeserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Directeur commercial"/>
    <many-to-one name="dirAgenceUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Directeur d'Agence"/>
    <many-to-one name="respCDSUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Responsable CDS"/>
    <many-to-one name="respAvtVenteUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Respondable AVANT-VENTE"/>
    <string name="pilotageEquipe" title="Pilotage, équipe"/>
    <date name="partTechniqueDate" title="Date Partie Technique"/>
    <date name="partCommercialeDate" title="Date Partie Commerciale"/>
    <date name="relectureDate" title="Date relecture"/>
    <date name="responseDate" title="Date de réponse"/>
    <string name="chargeAV" title="Charge AV"/>
    
    <!-- Onglet Synthèse de l'offre -->
    <many-to-one name="avUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="AVV"/>
    <string name="cdsConcerne" title="CDS concerné"/>
    <string name="descriptionProjet" title="Description du projet"/>
    <one-to-many name="offerLineList" mappedBy="affair" ref="com.axelor.apps.organisation.db.OfferLine" title="Lignes d'offres"/>
	
	<string name="fileEnclosed" title="File"/>
  </entity>

</domain-models>
