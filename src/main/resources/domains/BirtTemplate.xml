<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="BirtTemplate" lang="java">
  	
  	<string name="name" title="Name"/>
  	<string name="templateLink" title="Template file name"/>
  	<string name="format" title="Format" help="Pdf"/>
  	<string name="generatedFilePath" title="Generated file path" help="Example : /home/axelor/temp/"/>
  	<one-to-many name="birtTemplateParameterList" ref="com.axelor.apps.base.db.BirtTemplateParameter" mappedBy="birtTemplate" title="Parameters"/>
    
  </entity>

</domain-models>
