<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="SpentTime" lang="java">
  
  	<many-to-one name="event" ref="com.axelor.apps.crm.db.Event" title="Ticket"/>
  	<boolean name="timesheetImputed" title="Already imputed on timesheet" readonly="true"/>
  	<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" title="User"/>
  	<many-to-one name="task" ref="com.axelor.apps.organisation.db.Task" title="Task"/>
    <decimal name="duration" title="Duration" />
    <date name="date" title="Date"/>
    <many-to-one name="unit" title="Unit" ref="com.axelor.apps.base.db.Unit" required="true"/>
  </entity>

</domain-models>
