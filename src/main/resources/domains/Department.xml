<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

	<module name="base" package="com.axelor.apps.base.db"/>

	<entity name="Department" lang="java">

	<integer name="importId"/>
	<string name="name" title="Department Name" required="true"/>
	<string name="code" title="DEPT" required="true" unique="true"/>
	<many-to-one name="region" ref="com.axelor.apps.base.db.Region" title="Region"/>
	<one-to-many name="communeList" ref="Commune" mappedBy="department" title="Department Communes List"/>
	<one-to-many name="cantonList" ref="Canton" mappedBy="department" title="Department Cantons List"/>

	<many-to-one name="departmentParameter" ref="com.axelor.apps.account.db.Parameter" title="Pricing list parameter"/>

	</entity>

</domain-models>