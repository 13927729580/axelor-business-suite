<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ProductionConfig" lang="java">
  	<integer name="importId"/>
    <string name="name" title="Name" required="true"/>
    <string name="code" title="Code" unique="true"/>
    
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company" unique="true"/>

   	<many-to-one name="productionVirtualLocation" ref="com.axelor.apps.supplychain.db.Location" title="Production virtual location"/>
    <many-to-one name="prodResourceProduct" ref="com.axelor.apps.base.db.Product" title="Default resource product"/>
  	
  </entity>
  
</domain-models>
