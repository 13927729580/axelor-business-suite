<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

	<module name="consulting" package="com.axelor.apps.consulting.db"/>
	
	<entity name="NeedLine" lang="groovy">
		<many-to-one name="need" ref="com.axelor.apps.consulting.db.Need"/>
		<many-to-one name="product"	ref="com.axelor.apps.base.db.Product" title="Profil"/>
		<decimal name="price" title="Price"/>
<!-- 		 <![CDATA[ -->
<!-- 		 	return costLine != null? costLine.price : product.salePrice -->
<!-- 	    ]]>	</string> -->
		<string name="workLoad" title="Charge(%)" default="%"/>
		<datetime name="startDateTime" title="Start date"/>
		<datetime name="endDateTime" title="End date"/>
		<integer name="duration" title="Duration (Qty)"/>
		<many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit"/>
		<one-to-many name="resourceList" ref="com.axelor.apps.consulting.db.Resource" mappedBy="needLine" title="Ressources"/>
	</entity>
</domain-models>
