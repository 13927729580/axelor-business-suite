<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="organisation" package="com.axelor.apps.organisation.db"/>

  <entity name="Expense" lang="java">
  	<many-to-one name="employee" title="Employee" ref="com.axelor.apps.organisation.db.Employee"/>
  	<many-to-one name="userInfo" title="Employee" ref="com.axelor.apps.base.db.UserInfo"/>
	<date name="date" title="Date"/>
	<decimal name="totalAmount" title="Total amount" />   
    <many-to-one name="currency" ref="com.axelor.apps.base.db.Currency" title="Currency" />
    <one-to-many name="expenseLineList" ref="com.axelor.apps.organisation.db.ExpenseLine" mappedBy="expense" title="Expense lines"/>
    <string name="validationStatusSelect" title="Validation Status" selection="hr.expense.validation.status.select" readonly="true"/>
    <decimal name="progress" title="Progress" />
    <many-to-one name="paymentPeriod" ref="com.axelor.apps.base.db.Period" title="Payment Period" />
    <integer name="statusSelect" title="Status" selection="hr.expense.status.select" readonly="true"/>
    <many-to-one name="validatedByUserInfo" ref="com.axelor.apps.base.db.UserInfo" readonly="true" title="Validated by"/> 
  	<date name="validationDate" title="Validation date" readonly="true"/>
  	<string name="refusalReason" title="Refusal reason"/>
  	<many-to-one name="project" title="Affair" ref="com.axelor.apps.organisation.db.Project" />
  	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
  	
  </entity>
  
</domain-models>
