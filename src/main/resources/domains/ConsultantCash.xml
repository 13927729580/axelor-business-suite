<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

	<module name="organisation" package="com.axelor.apps.organisation.db" />

 	<entity name="ConsultantCash" lang="java">
 		<integer name="importId"/>
		<string name="label" title="Label"/>
		<string name="name" title="Title"/>
		<decimal name="amount" title="Amount"/>
		<date name="date" title="Date"/>
		<integer name="typeSelect" title="Type" selection="organisation.consultant.type.select"/>
		<many-to-one name="employee" ref="com.axelor.apps.organisation.db.Employee" title="Employee"/>
	</entity>

</domain-models>
