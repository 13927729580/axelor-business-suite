<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="SalesOrderScheduleLine" lang="java">
  
  	<many-to-one name="salesOrder" ref="com.axelor.apps.supplychain.db.SalesOrder" title="Devis"/>
  	<string name="sequence" title="Sequence"/>
  	<decimal name="amountToBeInvoiced" title="Montant à facturer"/>
  	<decimal name="invoicingPercentage" title="%"/>
  	<many-to-one name="salesOrderLine" ref="com.axelor.apps.supplychain.db.SalesOrderLine" title="Tâche" help="Si facturation à la tâche"/>
  	<decimal name="accumulatedInvoicingPercentage" title="% cumulé de facturation"/>
  	<integer name="nbOfDay" title="Nombre de jours" help="Si facturation en fonction de nb de jours"/>
  	<many-to-one name="invoice" ref="com.axelor.apps.account.db.Invoice" title="Facture"/>
    
  </entity>

</domain-models>
