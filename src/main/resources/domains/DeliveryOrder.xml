<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="DeliveryOrder" lang="java">
  
  	<many-to-one name="supplierPartner" ref="com.axelor.apps.base.db.Partner" title="Fournisseur"/>
  	<many-to-one name="affair" ref="com.axelor.apps.organisation.db.Project" title="Affaire"/>
  	<one-to-many name="deliveryOrderLineList" ref="com.axelor.apps.supplychain.db.DeliveryOrderLine" title="Lignes de commande"/>
  	<integer name="statusSelect" title="Etat" selection="default.status.select"/>
  	
  </entity>

</domain-models>
