<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />

	<entity name="SnSearch" lang="java">
		<boolean name="isCompany" title="Company ?"/>
		<string name="searchparam" help="Search Keyword"/>
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" help="Currently Active ERP User"/>
		<one-to-many name="snSearchResultList" ref="com.axelor.apps.socialnetwork.db.SnSearchResult" mappedBy="snSearch" help="Search Result of The Entered Keyword"/>
	</entity>
</domain-models>

