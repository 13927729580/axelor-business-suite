<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="InvoiceLineTax" lang="java">
  
    <many-to-one name="tax" ref="com.axelor.apps.account.db.Tax" title="Tax"/>
    <decimal name="base" title="Base"/>
    <decimal name="rateTax" precision="20" scale="6" title="Rate"/>
    <many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit" />
    <decimal name="exTaxTotal" title="Total W.T." precision="20" scale="2"/>
    <decimal name="accountingExTaxTotal" title="Total W.T." precision="20" scale="2"/>
    <many-to-one name="vatLine" ref="com.axelor.apps.account.db.VatLine" title="V.A.T."/>
    <many-to-one name="invoice" ref="Invoice" title="Invoice"/>
    <string name="baseOnSelect" title="Based on ?" selection="base.base.on.select"/>
    <many-to-one name="pricingListVersion" ref="com.axelor.apps.account.db.PricingListVersion" title="Pricinglist version"/>
      
  </entity>

</domain-models>
