<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="Machine" lang="java">
  	
  	<integer name="importId"/>
  	
  	<string name="name" title="Name"/>
    <string name="serialNumber" title="Serial number"/>
    <string name="description" title="Description" large="true"/>
    <many-to-one name="location" ref="com.axelor.apps.supplychain.db.Location" title="Location"/>
    <integer name="startingDuration" title="Starting duration (min)"/>
    <integer name="setupDuration" title="Setup duration (min)"/>
    
<!--     TODO A DEPLACER DANS LE MODULE BASE -->
    <many-to-one name="planning" ref="com.axelor.apps.organisation.db.Schedule" title="Planning"/>
    
  </entity>
</domain-models>
