<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="FBNotification">
		<string name="notificationRef" title="Notification ID" help="Unique Notification ID"/>
		<string name="title" title="Content of Update" help="Content of Notification"/>
		<string name="link" title="Direct Link To Update" help="Link To Content"/>
		<string name="updateTime" title="Time of Update" help="Time of Notification Creation"/>
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" help="Currently Active ERP User"/>
	</entity>
</domain-models>
