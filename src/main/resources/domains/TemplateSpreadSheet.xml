<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="base" package="com.axelor.apps.base.db" />
	<entity name="TemplateSpreadSheet">
	
			<string name="templateName" title="Template Name" unique="true" namecolumn="true" help="Template Spreadsheet Name"/>
			<many-to-one name="creatorUser" ref="com.axelor.auth.db.User" help="creator of Template" />
			<many-to-one name="templateModel" ref="com.axelor.meta.db.MetaModel" title="Model" orphan="true" help="Model"/>
			<many-to-many name="templateFields" ref="com.axelor.meta.db.MetaField" title="Template Fields"/>		
			<one-to-many name="criteria" ref="com.axelor.apps.base.db.TemplateSpreadSheetCriteria" mappedBy="templateSpreadSheet" help="Criteria to Export SpreadSheet" />
			<boolean name="share" default="false" />
			
	</entity>
</domain-models>

