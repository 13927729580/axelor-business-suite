<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="FBPagePost">
		<many-to-one name="fbPages" title="Page" ref="com.axelor.apps.socialnetwork.db.FBPages"
			orphan="true" help="Page on Which Post need to Create" />
		<string name="content" title="Content" max="1000" multiline="true" help="Content To post on Page"/>
		<string name="acknowledgment" title="Acknowledgment" help="Unique ID return by FB on Successfull Post"/>
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" help="Currently Active ERP User" />
		<datetime name="postedDateT" tz="true" help="Date and Time of Post" title="Posted date"/>
		<one-to-many name="fbPageCommentList" ref="com.axelor.apps.socialnetwork.db.FBPageComment"
			mappedBy="fbPagePost" help="Comments Received on Posted Content" title="Page comment list" />
	</entity>
</domain-models>
