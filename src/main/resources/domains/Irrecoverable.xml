<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="Irrecoverable" lang="java">
  
  	<string name="name" title="Name" readonly="true"/>
  	<one-to-many name="irrecoverableCustomerLineList" mappedBy="irrecoverable" ref="com.axelor.apps.account.db.IrrecoverableCustomerLine" title="Customer(s)"/>
  	<many-to-many name="invoiceSet" ref="com.axelor.apps.account.db.Invoice" title="Invoice &amp; Rejects"/>
  	<many-to-many name="paymentScheduleLineSet" ref="com.axelor.apps.account.db.PaymentScheduleLine" title="Payment schedule(s) rejected"/>
  	<many-to-many name="moveSet" ref="com.axelor.apps.account.db.Move" title="Moves generated"/>
  	<many-to-one name="status" ref="com.axelor.apps.base.db.Status" title="Status" readonly="true"/>
  	<datetime name="dateTime" title="Date" tz="true" readonly="true"/>
  	<many-to-one name="user" column="user_id" ref="com.axelor.auth.db.User" title="User" readonly="true"/>
  	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company" required="true"/>
  	<string name="exportTypeSelect" title="Export file type" selection="iadministration.export.type.select"/>
  
  </entity>

</domain-models>
