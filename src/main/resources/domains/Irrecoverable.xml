<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="Irrecoverable" lang="java">
  
  	<string name="name" title="Nom" readonly="true"/>
  	<one-to-many name="irrecoverableCustomerLineList" mappedBy="irrecoverable" ref="com.axelor.apps.account.db.IrrecoverableCustomerLine" title="Clients"/>
  	<many-to-many name="invoiceSet" ref="com.axelor.apps.account.db.Invoice" title="Factures et rejets de factures"/>
  	<many-to-many name="paymentScheduleLineSet" ref="com.axelor.apps.account.db.PaymentScheduleLine" title="Echéance rejetées"/>
  	<many-to-many name="moveSet" ref="com.axelor.apps.account.db.Move" title="Ecritures générées"/>
  	<many-to-one name="status" ref="com.axelor.apps.base.db.Status" title="Statut" readonly="true"/>
  	<datetime name="dateTime" title="Date" tz="true" readonly="true"/>
  	<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" title="Utilisateur" readonly="true"/>
  	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Société" required="true"/>
  	<string name="exportTypeSelect" title="Type d'impression" selection="iadministration.export.type.select"/>
  
  </entity>

</domain-models>
