<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="organisation" package="com.axelor.apps.organisation.db"/>

  <entity name="InterventionType">
    <string name="name" title="Intervention type"/>
    <string name="code" title="Code"/>
  </entity>

  <entity name="CompetenceArea">
    <string name="name" title="Competence Area"/>
    <string name="code" title="Code"/>
  </entity>

  <entity name="SkillLine">
	<string name="name" title="Label"></string>
	<string name="value" title="Value"></string>
	<many-to-one name="skillSet" ref="SkillSet"></many-to-one>
  </entity>

  <entity name="SkillSet">
	  <one-to-many name="skillLineList" ref="SkillLine" mappedBy="skillSet" title="Skills list" />
	  <many-to-many name="languageSet" ref="com.axelor.apps.base.db.Language" />
	  <many-to-one name="expertCompetence" ref="Competence"></many-to-one>
	  <many-to-one name="masteredCompetence" ref="Competence"></many-to-one>
	  <many-to-one name="awareCompetence" ref="Competence"></many-to-one>
  </entity>
  
  <entity name="Competence">
    <string name="typeSelect" selection="competence-type-select" title="Competence type"/>
    <many-to-one name="candidate" ref="Candidate"></many-to-one>
    <many-to-one name="employee" ref="Employee"></many-to-one>
    <string name="name" title="Intitulé du Profil"/>
    <string name="subTitle" title="Sous-titre du Profil"/>
    <many-to-many name="interventionTypeSet" ref="InterventionType" title="Domaines de Compétences"/>
    <many-to-many name="competenceAreaSet" ref="CompetenceArea" title="Types d'Intervention"/>
    <many-to-one name="expertSkillSet" ref="SkillSet" mappedBy="expertCompetence"></many-to-one>
    <many-to-one name="masteredSkillSet" ref="SkillSet" mappedBy="masteredCompetence"></many-to-one>
    <many-to-one name="awareSkillSet" ref="SkillSet" mappedBy="awareCompetence"></many-to-one>
  </entity>
</domain-models>