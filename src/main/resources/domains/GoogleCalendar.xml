<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
	<module name="googleapps" package="com.axelor.apps.googleapps.db" />
	<entity name="GoogleCalendar">
		<string name="calendarId" title="Calendar Id"/>
		<string name="name" title="Name of Calendar"/>
		<string name="eventId" title="Event Id"/>
		<string name="summary" title="Event Summary" required="true"/>
		<string name="location" title="Location"/>
		<string name="emailId" title="Email Id"/>
		<datetime name="createDate" title="Create Date" />
		<datetime name="startDate" title="Start Date" />
		<datetime name="endDate" title="End date"/>
		<many-to-one name="user" column="user_id" ref="com.axelor.auth.db.User" title="User"/>
	</entity>
</domain-models>
