<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="Rum" lang="java" >
  
    <string name="rumNumber" title="N° de RUM" namecolumn="true" initParam="true"/>
    <string name="debtorName" title="Nom du débiteur" initParam="true"/>
    <many-to-one name="debtorAddress" ref="com.axelor.apps.base.db.Address" title="Adresse du débiteur" initParam="true"/>
    <date name="creationDate" title="Creation date" initParam="true"/>
    <date name="mandateSignatureDate" title="Date de signature du mandat" initParam="true"/>
    <many-to-one name="mandateSignatureCommune" ref="com.axelor.apps.base.db.Commune" title="Ville de signature du mandat" initParam="true"/>
    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Partner" initParam="true"/>
    
    <string name="lastNumberIbanBic" hidden="true" />
    <boolean name="isUpdating" hidden="true" />
    <many-to-one name="activeBankDetails" ref="com.axelor.apps.base.db.BankDetails"/>
    
    
  </entity>

</domain-models>
