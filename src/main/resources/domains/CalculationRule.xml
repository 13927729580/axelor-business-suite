<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="CalculationRule" lang="java" cachable="true">
  
    <string name="name" title="Name"/>
    <string name="code" title="Code"/>
    <many-to-one name="base" ref="Base" title="Base"/>
    <many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit"/>
    <string name="calculationRule" title="Calculation Rule" large="true"/>
    <one-to-many name="formulaControlList" ref="com.axelor.apps.base.db.FormulaControl" mappedBy="calculationRule" title="Formula Controls" />
 
  </entity>

</domain-models>
