<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="VatManagement" lang="java">
  
  	<string name="code" title="Code" namecolumn="true" required="true"/>
  	<one-to-many name="vatManagementLineList" ref="com.axelor.apps.account.db.VatManagementLine" mappedBy="vatManagement" title="V.A.T. management line" />
    <one-to-many name="formulaControlList" ref="com.axelor.apps.base.db.FormulaControl" mappedBy="vatManagement" title="Formula control list" />
  
  </entity>

</domain-models>
