<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="TemplateRule" lang="java">
  	
  	<string name="name" title="Name" namecolumn="true"/>
  	
  	<many-to-one name="metaModel" ref="com.axelor.meta.db.MetaModel" title="Model"/>
  	<one-to-many name="templateRuleLineList" ref="com.axelor.apps.base.db.TemplateRuleLine" orderBy="sequence" mappedBy="templateRule"/>
    
  </entity>

</domain-models>
