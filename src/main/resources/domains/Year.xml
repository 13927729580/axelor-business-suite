<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="Year" lang="java">
  
    <string name="name" title="Name" required="true"/>
    <string name="code" title="Code"/>
    <date name="fromDate" title="From"/>
    <date name="toDate" title="To"/>
    <date name="reportedBalanceDate" title="Reported balance Date"/>
    <one-to-many name="periodList" ref="com.axelor.apps.base.db.Period" mappedBy="year" title="Periods list"/>
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
    <many-to-one name="status" ref="com.axelor.apps.base.db.Status" title="Status" readonly="true"/>
    <integer name="periodDurationSelect" title="Period duration" selection="account.year.period.duration.select"/>
<!--     <one-to-many name="reportedBalanceLineList" ref="com.axelor.apps.account.db.ReportedBalanceLine" mappedBy="year" title="Reported balace lines"/> -->
  
  </entity>

</domain-models>
