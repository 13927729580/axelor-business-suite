<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db" />

  <entity name="LnNetworkUpdates">
	<string name="networkUpdateId" title="ID" />
	<string name="networkUpdateContent" title="Content" multiline="true" max="10000" />
	<datetime name="networkUpdateDateT" title="Time" tz="true" />
	<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" />
	<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" />
  </entity>
</domain-models>