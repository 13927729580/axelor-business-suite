<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="Inventory" lang="java">
  
  	<string name="name" title="Name"/>
  	<many-to-one name="location" ref="com.axelor.apps.supplychain.db.Location" title="Location"/>
    <integer name="statusSelect" title="Status" selection="supplychain.default.status.select" readonly="true"/>
  	<datetime name="dateT" title="Date" required="true" tz="true"/>
  	<string name="description" title="Description" large="true"/>
  	<one-to-many name="inventoryLineList" ref="com.axelor.apps.supplychain.db.InventoryLine" mappedBy="inventory" title="Inventory lines"/>
  	<integer name="formatSelect" title="Format" selection="supplychain.inventory.format.select"/>
  	
  </entity>
</domain-models>
