<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="BillOfMaterial" lang="java">
  
  	<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product"/>
  	<decimal name="qty" title="Quantity"/>
  	<many-to-one name="productVariant" ref="com.axelor.apps.base.db.ProductVariant" title="Product variant"/>
  	<integer name="priority" title="Priority"/>
    <boolean name="isRawMaterial" title="Raw material"/>
    <many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit"/>
    <many-to-one name="prodProcess" ref="com.axelor.apps.production.db.ProdProcess" title="Production process"/>
  	<many-to-many name="billOfMaterialSet" ref="com.axelor.apps.production.db.BillOfMaterial" title="BOMs"/>
  	<one-to-many name="prodRemainsList" ref="com.axelor.apps.production.db.ProdRemains" title="Production remains"/>
  	<string name="name" title="Label"/>
    
  </entity>
</domain-models>
