<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.8.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="MsgInbox">
		<string name="messageRef" />
		<string name="snUserRef" />
		<string name="messageFrom" />
		<string name="messageContent" multiline="true" max="5000"/>
		<datetime name="messageDateT" tz="true" title="Message date" />
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" />
		<many-to-one name="socialNet" ref="com.axelor.apps.socialnetwork.db.SocialNet"/>
	</entity>
</domain-models>

