<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="InvoiceLineTaxHistory" lang="java">
  
    <many-to-one name="invoice" ref="Invoice" title="Invoice"/>
    <many-to-one name="invoiceLine" ref="InvoiceLine" title="Invoice line"/>
    <date name="fromDate" title="From"/>
    <date name="toDate" title="To"/>
    <many-to-one name="tax" ref="com.axelor.apps.account.db.Tax" title="Tax"/>
    <decimal name="base" title="Base"/>
    <decimal name="rateTax" title="Rate" scale="6" precision="20"/>
    <many-to-one name="vatLine" ref="com.axelor.apps.account.db.VatLine" title="V.A.T."/>
    <many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit"/>
    <integer name="typeSelect" title="Type" selection="invoice.line.tax.history.type.select"/>
    <many-to-one name="taxLine" ref="com.axelor.apps.account.db.TaxLine" title="Ligne de taxe"/>
    <string name="baseOnSelect" title="Basée sur ?" selection="base.base.on.select"/>
    <boolean name="fromInvoiceOk" title="Issue de la refacturation ?"/>
    <many-to-one name="pricingListVersion" ref="com.axelor.apps.account.db.PricingListVersion" title="Version de Barème"/>
      
  </entity>
  
</domain-models>
