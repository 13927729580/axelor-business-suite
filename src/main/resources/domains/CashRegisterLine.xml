<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
  
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="CashRegisterLine" lang="java">
    
    <string name="name">
		<![CDATA[
		if (cashRegister != null && cashRegisterDate != null)
			return cashRegister.getName() + " " + cashRegisterDate.toString();
		else  return "-";
		]]>
 	</string>
    <many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" title="Cashier Agent" readonly="true"/>
    <many-to-one name="cashRegister" ref="com.axelor.apps.account.db.CashRegister" title="CashRegister" required="true"/>
    <datetime name="createDateTime" title="Creation Date" tz="true" readonly="true"/>
    <date name="cashRegisterDate" title="Cash register date" required="true"/>
    <decimal name="depositedAmount" title="Deposited Amount"/>
    <decimal name="cashRegisterAmount" title="Cash Regsiter Amount"/>
    <integer name="stateSelect" title="Status" readonly="true" selection="cash.register.state.select"/>
    <boolean name="emailSendedOk" title="Email sent ?" default="false" readonly="true"/>
  
  </entity>
  
</domain-models>
