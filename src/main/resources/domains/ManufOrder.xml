<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ManufOrder" lang="java">
  
<!--   	Not display -->
	<many-to-one name="manufOrderPlanning" ref="com.axelor.apps.production.db.ManufOrderPlanning" title="Manufacturing order planning"/>
  
    <string name="manufOrderSeq" title="MO Sequence"/>
    <many-to-one name="billOfMaterial" ref="com.axelor.apps.production.db.BillOfMaterial" title="BOM"/>
    <many-to-one name="prodProcess" ref="com.axelor.apps.production.db.ProdProcess" title="Production process"/>
    <one-to-many name="inStockMoveList" ref="com.axelor.apps.supplychain.db.StockMove" title="Stock moves in"/>
    <many-to-one name="outStockMove" ref="com.axelor.apps.supplychain.db.StockMove" title="Stock moves out"/>
    
    <many-to-one name="inLocation" ref="com.axelor.apps.supplychain.db.Location" title="Location in"/>
    <many-to-one name="outLocation" ref="com.axelor.apps.supplychain.db.Location" title="Location out"/>
    
    <date name="startDate" title="Start date"/>
    <date name="endDate" title="End date"/>
    
    <boolean name="isSimulation" readonly="true" title="Simulation ?"/>
    
    <one-to-many name="manufOrderLineList" ref="com.axelor.apps.production.db.ManufOrderLine" mappedBy="manufOrder" title="Manufacturing order lines"/>
    
    <integer name="statusSelect" title="Status" selection="production.prod.order.status.select"/>
    
  </entity>
</domain-models>