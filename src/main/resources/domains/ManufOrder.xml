<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ManufOrder" lang="java">
  
<!--   	Not display -->
	<many-to-one name="manufOrderPlanning" ref="com.axelor.apps.production.db.ManufOrderPlanning" title="Manufacturing order planning"/>
  
  	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
  
    <string name="manufOrderSeq" title="MO Sequence" namecolumn="true"/>
    <many-to-one name="billOfMaterial" ref="com.axelor.apps.production.db.BillOfMaterial" title="BOM"/>
    
<!--     Champ pointÃ© ? -->
    <many-to-one name="prodProcess" ref="com.axelor.apps.production.db.ProdProcess" title="Production process"/> 
    
    <datetime name="plannedStartDateT" title="Planned start date"/>
  	<datetime name="plannedEndDateT" title="Planned end date"/>
  	
 	<datetime name="realStartDateT" title="Real start date"/>
  	<datetime name="realEndDateT" title="Real end date"/>
    
    <many-to-one name="inStockMove" ref="com.axelor.apps.supplychain.db.StockMove" title="Stock move in"/>
    <many-to-one name="outStockMove" ref="com.axelor.apps.supplychain.db.StockMove" title="Stock move out"/>
    
    <many-to-one name="wasteStockMove" ref="com.axelor.apps.supplychain.db.StockMove" title="Stock move out"/>
    
    <string name="wasteProdDescription" title="Description" large="true"/>
    
    <decimal name="wasteProdQty" title="Quantity"/>
    
    <integer name="statusSelect" title="Status" selection="production.manuf.order.status.select"/>
    
  </entity>
</domain-models>