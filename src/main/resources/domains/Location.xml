<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="Location" lang="java">
  
    <string name="name" title="Nom" required="true"/>
    <many-to-one name="parent" ref="com.axelor.apps.supplychain.db.Location" title="Parent"/>
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Partner"/>
    <integer name="typeSelect" title="Type" selection="supplychain.location.type.select"/>
    
    <one-to-many name="locationLineList" ref="com.axelor.apps.supplychain.db.LocationLine" mappedBy="location" title="Location lines"/>
    
  </entity>
</domain-models>
