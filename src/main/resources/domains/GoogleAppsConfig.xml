<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.0.xsd">
	<module name="base" package="com.axelor.apps.base.db" />
		<entity name="GoogleAppsConfig" sequential="true" lang="groovy">
		
			<string name="clientEmail"  required="true" title="Client Email Id" help="Client Email Id in Api Console"/>
			<string name="clientId" required="true" title="Client Id" help="Cliend Id in Api Console " />
			<string name="clientSecret" required="true" title="Client Secret" help="Client Secret in Api Console" />
			<string name="redirectURI" required="true" title="Redirect Uri" help="Redirect Url to get auth code"/>
		
			<!-- Authorization -->
			<many-to-one name="driveUserInfo" ref="com.axelor.apps.base.db.UserInfo" title="Drive User"  help="Google Apps User "/>		
			<string name="accessToken" required="false" title="Access Token" help="Access Token retrived by google" />
			<string name="refreshToken" required="false"  title="Refresh Token" help="Refresh Token retrived by Google to get Access Token" />
			<string name="lastAccessTime" required="false" title="Last Access Time" help="last Access time of Authorization"/>
		
		</entity>
</domain-models>
