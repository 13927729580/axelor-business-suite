<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="FBPageComment">
		<string name="commentid" title="Comment id" help="Facebook Unique ID for Comment"/>
		<many-to-one name="fbPagePost" ref="com.axelor.apps.socialnetwork.db.FBPagePost"
			title="Please Select Message" help="Posted Content Association on which Comment is Being Received"/>
		<string name="from_user" title="From::User" help="Comment Made by User" />
		<string name="commentTime" title="Comment Time" help="Comments Date and Time"/>
		<string name="comment" title="Comment" help="Comments Content"/>
		<string name="commentLikes" title="Likes" help="Likes on Comment"/>
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" help="Currently Active ERP User"/>
	</entity>
</domain-models>

