<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="VatManagementLine" lang="java">
  
  	<many-to-one name="vatManagement" ref="com.axelor.apps.account.db.VatManagement" title="V.A.T. Management" />
  	<string name="condition" title="Application condition" max="2048" required="true"/>
  	<many-to-one name="vat" ref="com.axelor.apps.account.db.Vat" title="V.A.T." required="true"/>
  
  </entity>

</domain-models>
