<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
 	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
 	<entity name="SnSendMessage">
 		<string name="content" multiline="true" max="140"/>
 		<string name="snMessageRef" />
 		<string name="subject" />
 		<datetime name="messageDateT" tz="true" title="Message date" />
 		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" />
 		<many-to-one name="socialNet" ref="com.axelor.apps.socialnetwork.db.SocialNet" orphan="true" />
 		<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" orphan="true" />
 		<many-to-one name="personalCredential" ref="com.axelor.apps.socialnetwork.db.PersonalCredential" />
 		<many-to-one name="socialNetUserAccount" title="Social Network Account" ref="com.axelor.apps.socialnetwork.db.SocialNetUserAccount"/>
 	</entity>
 </domain-models>
