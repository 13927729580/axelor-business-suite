<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="SocialNet">
		<![CDATA[
	    This class references the social networks.
	    It indicates their name and website.
	    
	    For example :
	    name : facebook
	    website : www.facebook.com
	    @author David Nguyen
	    ]]>
		<string name="name" title="Name" help="true"/>
		<string name="website" title="Website" help="true"/>
	</entity>
</domain-models>