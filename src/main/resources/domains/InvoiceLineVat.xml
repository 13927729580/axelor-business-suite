<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="InvoiceLineVat" lang="java">
  
    <many-to-one name="invoice" ref="Invoice" title="Invoice"/>
    <decimal name="exAllTaxBase" scale="2" precision="20" title="Base W.A.T."/>
    <decimal name="exTaxBase" scale="2" precision="20" title="Base W.T."/>
    <decimal name="vatTotal" scale="2" precision="20" title="Amount V.A.T."/>
    <decimal name="inTaxTotal" scale="2" precision="20" title="Amount A.T.I."/>
    <decimal name="accountingExAllTaxBase" scale="2" precision="20" title="Base W.A.T. in accounting currency" hidden="true"/>
    <decimal name="accountingExTaxBase" scale="2" precision="20" title="Base W.T. in accounting currency" hidden="true"/>
    <decimal name="accountingVatTotal" scale="2" precision="20" title="Amount V.A.T. in accounting currency" hidden="true"/>
    <decimal name="accountingInTaxTotal" scale="2" precision="20" title="Amount A.T.I. in accounting currency" hidden="true"/>
    <many-to-one name="vatLine" ref="com.axelor.apps.account.db.VatLine" title="V.A.T."/>
  
  </entity>

</domain-models>
