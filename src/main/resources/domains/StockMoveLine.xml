<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="StockMoveLine" lang="java">
  
  	<many-to-one name="stockMove" ref="com.axelor.apps.supplychain.db.StockMove" title="Stock move"/>
  	
  	<integer name="importId" />
  	
	<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product"/>    
	<decimal name="qty" title="Qty"/>
	<decimal name="realQty" title="RealQty"/>
    <many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit"/>
    <decimal name="price" title="Unit price"/>
    
    <many-to-one name="trackingNumber" ref="com.axelor.apps.base.db.TrackingNumber" title="Tracking Nbr."/>
    
    <many-to-one name="salesOrderLine" ref="com.axelor.apps.supplychain.db.SalesOrderLine" title="Sales order line"/>
    <many-to-one name="purchaseOrderLine" ref="com.axelor.apps.supplychain.db.PurchaseOrderLine" title="Purchase order line"/>
    
<!--     TODO a deplacer dans Production -->
	<many-to-one name="consumedManufOrder" ref="com.axelor.apps.production.db.ManufOrder" title="Manufacturing order"/>
	<many-to-one name="producedManufOrder" ref="com.axelor.apps.production.db.ManufOrder" title="Manufacturing order"/>
	
	<many-to-one name="consumedOperationOrder" ref="com.axelor.apps.production.db.OperationOrder" title="Operation order"/>
    
<!--     TODO a deplacer dans Supplier management -->
    <decimal name="shippedQty" title="Shipped qty"/>
    <date name="shippedDate" title="Shipped date"/>
    
  </entity>
</domain-models>
