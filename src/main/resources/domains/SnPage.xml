<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="socialnetwork" package="com.axelor.apps.socialnetwork.db"/>

  <entity name="SnPage">
  	<![CDATA[
    This class represents a page on Facebook
 	A user can have many pages and a page can be related to multiple user
 	so there is a many2many relation with SnProfile
    @author David Nguyen
    ]]>
  	<many-to-one name="user" ref="com.axelor.auth.db.User"/>
  	<string name="pageRef"/>
  	<string name="name" title="Name"/>
  	<string name="userName"/>
  	<many-to-many name="snProfileSet" ref="com.axelor.apps.socialnetwork.db.SnProfile"/>
  	<string name="link" title="Link"/>
  	<string name="pictureUrl"/>
  </entity>
 
</domain-models>