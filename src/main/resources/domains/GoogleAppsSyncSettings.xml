<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">
	<module name="googleapps" package="com.axelor.apps.googleapps.db" />
		<entity name="GoogleAppsSyncSettings" sequential="true" >
					<integer name="filesModifiedWithin"  required="true" title="Get Files Modified Within" />
					<boolean name="getSharedWithMeFiles" required="true" title="Get Files Shared With Me"  />
					<many-to-one name="driveUser" ref="com.axelor.auth.db.User" orphan="true"  />
		</entity>
</domain-models>
