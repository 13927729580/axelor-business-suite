<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="base" package="com.axelor.apps.base.db" />
	<entity name="PersonalCredential">
		<many-to-one name="socialNet" ref="com.axelor.apps.base.db.SocialNet"
			title="Network Type" required="true" help="Network Type"></many-to-one>
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo"
			title="User" required="true" help="Current Active ERP User"></many-to-one>
		<string name="publicUserToken" title="User Token" required="true" help="User Token To Access Protected Resources"></string>
		<string name="secretUserToken" title="User Token Secret" help="User Token Secret Reuired To Access Protected Resources"
			default="null" readonly="true"></string>
		<string name="snUserName" title="Username" readonly="true" help="User Name who is Granted Access"></string>
	</entity>
</domain-models>
