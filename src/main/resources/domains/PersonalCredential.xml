<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="PersonalCredential">
		<![CDATA[
	    This class is used to store public and secret user's keys.
	    Those keys are unique and obtained from application authorization.
	    @author David Nguyen
	    ]]>
		<many-to-one name="socialNet" ref="com.axelor.apps.socialnetwork.db.SocialNet"
			title="Network Type" required="true" help="true"/>
		<string name="publicUserToken" title="User Token" required="true" help="true"/>
		<string name="secretUserToken" title="User Token Secret" help="true"
			default="null" readonly="true"/>
		<string name="snUserName" title="Username" readonly="true" help="true"/>
	</entity>
</domain-models>
