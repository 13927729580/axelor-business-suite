<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

	<module name="base" package="com.axelor.apps.base.db"/>
	
	<entity name="FilterList">
	
		<long name="recordId"/>
		<string name="redordModel"/>
		<long name="filterId"/>
		<string name="filterCode"/>
		
		<finder-method name="findByFilterCode" using="long:id" filter="self.filterCode = :filterCode" all="true"/>
		<finder-method name="findByFilterId" using="long:id" filter="self.filterId = :id" all="true"/>
		
	</entity>
	
</domain-models>
