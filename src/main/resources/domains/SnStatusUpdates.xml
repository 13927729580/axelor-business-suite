<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="SnStatusUpdates">
  	<string name="linkedinUpdateId"/>
  	<string name="facebookUpdateId"/>
  	<string name="twitterUpdateId"/>
    <string name="updateContent" namecolumn="true" multiline="true" max="600"/>
    <datetime name="updateTime" tz="true"/>
    <many-to-one name="curUser" ref="com.axelor.auth.db.User"/>
    <one-to-many name="commentList" ref="com.axelor.apps.base.db.SnStatusComments" orphan="true" mappedBy="updateId"/>
 </entity>
 
</domain-models>