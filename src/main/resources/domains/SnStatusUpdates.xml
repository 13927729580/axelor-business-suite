<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">

  <module name="socialnetwork" package="com.axelor.apps.socialnetwork.db"/>

  <entity name="SnStatusUpdates">
  	<string name="linkedinUpdateRef"/>
  	<string name="facebookUpdateRef"/>
  	<string name="twitterUpdateRef"/>
    <string name="content" namecolumn="true" multiline="true" max="600"/>
    <datetime name="updateDateT" tz="true"/>
    <many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo"/>
    <one-to-many name="snStatusCommentsList" ref="com.axelor.apps.socialnetwork.db.SnStatusComments" orphan="true" mappedBy="snStatusUpdates"/>
 </entity>
 
</domain-models>