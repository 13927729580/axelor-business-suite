<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="socialnetwork" package="com.axelor.apps.socialnetwork.db" />
	<entity name="FBSearchResult">
		<many-to-one name="fbSearch" ref="com.axelor.apps.socialnetwork.db.FBSearch"/>
		<string name="userRef" help="Facebook Generated Unique User's ID"/>
		<string name="firstName" help="First Name of Person" />
		<string name="lastName" help="Last Name of Person" />
		<string name="gender" />
		<string name="link" help="Profile Link" />
		<string name="companyName" help="Name of Company" />
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo"/>
 		<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" orphan="true" />
 		<!-- <binary name="picture" image="true" title="Photo"/> -->
 		<string name="pictureUrl" title="Picture Link"/>
	</entity>
</domain-models>
