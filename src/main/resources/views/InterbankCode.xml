<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    <grid name="interbank-code-grid" title="Blocking reason/reject codes lists" model="com.axelor.apps.account.db.InterbankCode">
        <field name="code"/>
        <field name="name"/>
        <field name="description"/>
    </grid>
     <grid name="interbank-code-line-custom-grid" title="Blocking reason/reject codes lists" model="com.axelor.apps.account.db.InterbankCodeLine"> 
		        <field name="code"/>
		        <field name="name"/>
		        <field name="description"/>
		        <field name="technicalRejectOk"/>
	</grid>
	    	<form model="com.axelor.apps.account.db.InterbankCodeLine" name="interbank-code-line-custom-form" title="Blocking reason/reject codes lists">
		        <group colSpan="4" cols="6">
		            <field name="code"/>
		            <field name="name"/>
		        </group>
		        <group title="Description" colSpan="4" cols="4">
		            <field name="description" showTitle="false" colSpan="4"/>
		            <field name="technicalRejectOk"/>
		        </group>
		        <group title="Domain of use" colSpan="4" cols="6">
		            <field name="transferCfonbOk"/>
		            <field name="directDebitAndTipCfonbOk"/>
		            <field name="directDebitSepaOk"/>
		            <field name="lcrBorOk"/>
		            <field name="chequeOk"/>
		        </group>
	  		</form>
    <form cols="6" name="interbank-code-form" title="Blocking reason/reject codes lists" model="com.axelor.apps.account.db.InterbankCode">
  <panel title="Overview">
    <field name="code"/>
    <field name="name"/>
    <field name="endDate"/>
  </panel>
  <panel title="Description" >
    <field name="description" showTitle="false" x-span="12"/>
  </panel> 
  <panel-related field="interbankCodeLineList" form-view="interbank-code-line-custom-form" grid-view="interbank-code-line-custom-grid"/>
    
</form>
    
</object-views>