<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_3.0.xsd">


	<form name="evaluation-line-form" title="Evaluation lines"
		model="com.axelor.apps.organisation.db.EvaluationLine">
		<panel>		
			<field name="label"/>
			<field name="coefficient"/>
			<field name="gradeSelect"/>
		</panel>
	</form>


	<grid name="evaluation-line-grid" title="Evaluation lines"
		model="com.axelor.apps.organisation.db.EvaluationLine" editable="true">
		<field name="label"/>
		<field name="coefficient"/>
		<field name="gradeSelect"/>
	</grid>


</object-views>