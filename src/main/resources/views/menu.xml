<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.0.xsd">
	
	
	<menuitem name="sc-root-purchase-order-supplier-lines" title="Supplier requests" priority="1" 
		groups="admins,erp" action="sc.root.purchase.orders.supplier.lines.requests" parent="sc-root-purchases"/>

	<action-view name="sc.root.purchase.orders.supplier.lines.requests" title="Supplier requests"
		model="com.axelor.apps.suppliermanagement.db.PurchaseOrderSupplierLine" icon="img/16px/orderSmall_16px.png">
		<view type="grid" name="purchase-order-supplier-line-supplier-grid" />
		<view type="form" name="purchase-order-supplier-line-supplier-form" />
		<domain>self.stateSelect = 1</domain>
	</action-view>
	
	<menuitem name="sc-root-purchase-order-supplier-lines" title="Supplier requests completed" priority="1" 
		groups="admins,erp" action="sc.root.purchase.orders.supplier.lines.requests" parent="sc-root-purchases"/>

	<action-view name="sc.root.purchase.orders.supplier.lines.requests" title="Supplier requests completed"
		model="com.axelor.apps.suppliermanagement.db.PurchaseOrderSupplierLine" icon="img/16px/orderSmall_16px.png">
		<view type="grid" name="purchase-order-supplier-line-supplier-grid" />
		<view type="form" name="purchase-order-supplier-line-supplier-form" />
		<domain>self.stateSelect = 2</domain>
	</action-view>
	
	<menuitem name="sc-root-purchase-order-supplier-lines" title="Supplier requests historical" priority="1" 
		groups="admins,erp" action="sc.root.purchase.orders.supplier.lines.requests" parent="sc-root-purchases"/>

	<action-view name="sc.root.purchase.orders.supplier.lines.requests" title="Supplier requests historical"
		model="com.axelor.apps.suppliermanagement.db.PurchaseOrderSupplierLine" icon="img/16px/orderSmall_16px.png">
		<view type="grid" name="purchase-order-supplier-line-supplier-grid" />
		<view type="form" name="purchase-order-supplier-line-supplier-form" />
	</action-view>

	
</object-views>