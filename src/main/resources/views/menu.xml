		<domain>self.typeSelect = 3 and self.team IN self.userInfo.teamSet</domain>
		<!--  <domain>self.typeSelect = 3 and self.team IN :_internalUserTeam</domain> -->
		<context name="_typeSelect" expr="3"/>
		<!-- <context name="_internalUserTeam" expr="eval: team?.userInfoSet?.find{ it.internalUser.id ==  __user__.id }?.teamSet"/>  --> 
	</action-view>
	
