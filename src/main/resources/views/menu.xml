		<view type="form" />
		<domain>self.userInfo IS NULL</domain>
	</action-view>

	<menuitem name="crm-root-team" parent="crm-root"
		title="Team"/>

	<menuitem name="crm-root-team-tasks" parent="crm-root-team"
