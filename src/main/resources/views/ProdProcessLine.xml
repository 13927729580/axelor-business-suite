<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="prod-process-line-grid" title="Prod. process lines" model="com.axelor.apps.production.db.ProdProcessLine">
        <field name="prodResource"/>
        <field name="capacityPerHour"/>
        <field name="hasStockCheckout"/>
        <field name="duration"/>
        <field name="nextProdProcessLine"/>
        <field name="previousProdProcessLine"/> 
    </grid>
    
    <form name="prod-process-line-form" title="Prod. process line" model="com.axelor.apps.production.db.ProdProcessLine" cols="4">
        <field name="prodResource"/>
        <field name="capacityPerHour"/>
        <field name="hasStockCheckout"/>
        <field name="duration"/>
        <field name="nextProdProcessLine"/>
        <field name="previousProdProcessLine"/>
    </form>
    
</object-views>
