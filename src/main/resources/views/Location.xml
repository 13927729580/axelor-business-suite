<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="location-grid" title="Stock Locations" model="com.axelor.apps.supplychain.db.Location">
        <field name="name"/>
        <field name="parent"/>
        <field name="company"/>
        <field name="partner"/>
        <field name="typeSelect"/>
    </grid>
    
    <form name="location-form" title="Stock Location" model="com.axelor.apps.supplychain.db.Location" cols="4"
    	onNew="action-location-record-new">
        <field name="name"/>
        <field name="parent"/>
        <field name="company"/>
        <field name="partner"/>
        <field name="typeSelect"/>
        <field name="locationLineList" colSpan="4"/>
    </form>
    
    <action-record name="action-location-record-new" model="com.axelor.apps.supplychain.db.Location">
	    <field name="typeSelect" expr="1"/>
	</action-record>
    
</object-views>
