<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="paybox-config-grid" title="Paybox configurations" model="com.axelor.apps.account.db.PayboxConfig">
        <field name="name"/>
        <field name="code"/>
        <field name="address"/>
    </grid>
    
    <form name="paybox-config-form" title="Paybox configuration" model="com.axelor.apps.account.db.PayboxConfig" cols="4">
	    
        <field name="name"/>
        <field name="code"/>
        
        <separator colSpan="4"/>
        
        <field name="payboxUrl"/>
       	<field name="payboxSite"/>
       	<field name="payboxRang"/>
       	<field name="payboxDevise"/>
       	<field name="payboxIdentifiant"/>
       	<field name="payboxHashSelect"/>
		<field name="payboxHmac"/>
		<field name="payboxDefaultEmail"/>
		<field name="payboxRetour"/>
       	<field name="payboxRetourUrlEffectue"/>
       	<field name="payboxRetourUrlRefuse"/>
       	<field name="payboxRetourUrlAnnule"/>
		<field name="payboxPublicKeyPath"/>
        
    </form>
	
	
</object-views>