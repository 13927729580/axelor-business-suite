<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_3.0.xsd">
    
    <grid name="schedule-sub-line-grid" title="Working timeshift periods" model="com.axelor.apps.organisation.db.ScheduleSubLine" editable="true">
        <field name="daySelect"/>
        <field name="startPeriod1"/>
        <field name="endPeriod1"/>
        <field name="startPeriod2"/>
        <field name="endPeriod2"/>
    </grid>
    
    <form name="schedule-sub-line-form" title="Working timeshift periods" model="com.axelor.apps.organisation.db.ScheduleSubLine">
		<panel>
	        <field name="daySelect"/>
	        <field name="startPeriod1"/>
	        <field name="endPeriod1"/>
	        <field name="startPeriod2"/>
	        <field name="endPeriod2"/>
        </panel>
    </form>
    
    
</object-views>