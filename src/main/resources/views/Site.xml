<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
	
	<grid name="site-grid" title="Site" model="com.axelor.apps.consulting.db.Site" >
		<field name="name"/>
	    <field name="partner"/>
	    <field name="siretNumber"/>
	    <field name="geographicalArea"/>
	</grid>
	
    <form name="site-form" title="Site" model="com.axelor.apps.consulting.db.Site" >
		<field name="name"/>
	    <field name="partner"/>
	    <field name="siretNumber"/>
	    <field name="address"/>
	    <field name="commercialLevelSelect"/>
	    <field name="geographicalArea"/>
	    <break/>
		<notebook colSpan="1" title="Notebook key words">
			<page title="Keyword tool">
				<field name="toolKeyWordSet" />
			</page>
			<page title="Keyword sector">
				<field name="sectorKeyWordSet" />
			</page>
		</notebook>
    </form>
	
</object-views>