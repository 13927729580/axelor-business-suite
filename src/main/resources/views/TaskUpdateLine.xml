<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="task-update-line-grid" title="Les mises à jours des coûts" model="com.axelor.apps.organisation.db.TaskUpdateLine">
        <field name="date"/>
        <field name="userInfo"/>
        <field name="revenue"/>
        <field name="cost"/>
    </grid>
    
    <form name="task-update-line-form" title="Mise à jour des coût" model="com.axelor.apps.organisation.db.TaskUpdateLine" cols="4" >
        <field name="date"/>
        <field name="userInfo"/>
        <field name="revenue"/>
        <field name="cost"/>
        <button name="update" title="Ajouter"/>
    </form>
    
    
</object-views>
