	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.0.xsd">
    <grid name="region-grid" title="Regions" model="com.axelor.apps.base.db.Region">
        <field name="code"/>
