<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    <grid name="commune-grid" title="Les communes" model="com.axelor.apps.base.db.Commune">
        <field name="name"/>
        <field name="inseeCode"/>
    </grid>
    <form name="commune-form" title="Commune" model="com.axelor.apps.base.db.Commune" cols="4">
        <field name="name"/>
        <field name="internalId"/>
        <field name="inseeCode"/>
        <field name="zip"/>
        <field name="department"/>
        <field name="nbInhCommune"/>
        <field name="canton"/>
        <field name="artmin"/>
    </form>
</object-views>
