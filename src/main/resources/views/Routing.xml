<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
	
	<grid name="routing-grid" title="Routing rules" model="com.axelor.apps.base.db.Routing">
		<field name="name"/>
		<field name="code"/>
		<field name="priority"/>
		<field name="isActive"/>
	</grid>
	
	<form name="routing-form" title="Routing rule" model="com.axelor.apps.base.db.Routing" >
		<panel>
			<field name="name"/>
			<field name="code"/>
			<field name="priority"/>
			<field name="isActive"/>
		</panel>
		<panel-tabs >
			<panel-related title="Filters" field="routingRuleList" />
			<panel-related title="Actions" field="routingActionList" />
		</panel-tabs>
	</form>

</object-views>