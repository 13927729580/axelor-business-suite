<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    <grid name="pitd-grid" title="Liste des PITD" model="com.axelor.apps.base.db.Pitd">
		<field name="name"/>
		<field name="code"/>
		<field name="regionalTransportLevel"/>
		<field name="weatherZone"/>
    </grid>
    <form name="pitd-form" title="PITD" model="com.axelor.apps.base.db.Pitd" cols="4">
		<field name="name"/>
		<field name="code"/>
		<field name="regionalTransportLevel"/>
		<field name="weatherZone"/>
    </form>
 
	
</object-views>
