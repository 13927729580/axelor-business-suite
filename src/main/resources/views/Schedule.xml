<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    
    <grid name="schedule-grid" title="Working Schedules" model="com.axelor.apps.organisation.db.Schedule">
        <field name="name"/>
        <field name="publicHolidays"/>
    </grid>
    
    <form name="schedule-form" title="Working Schedule" model="com.axelor.apps.organisation.db.Schedule" >
        <panel>
	        <field name="name"/>
	        <field name="publicHolidays"/>
	        <panel-related field="scheduleLineList" x-span="12"/>
        </panel>
    </form>
    
    
</object-views>