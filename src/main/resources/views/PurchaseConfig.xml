<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    
    <grid name="purchase-config-grid" title="Purchase configurations" model="com.axelor.apps.purchase.db.PurchaseConfig">
        <field name="company"/>
    </grid>

    <form name="purchase-config-form" title="Purchase configuration" model="com.axelor.apps.purchase.db.PurchaseConfig">
		<panel title="Overview">
		  <field name="company" form-view="company-lite-form" colSpan="4"/>
		</panel>
		<panel title="Invoicing Information">
		  <field name="purchaseOrderInvoicingTypeSelect"/>
		</panel>
    </form>
	
	
</object-views>