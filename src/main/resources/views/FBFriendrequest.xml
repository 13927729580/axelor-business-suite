<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">

	<grid title="Friend Request" name="fb-req-grid" model="com.axelor.apps.base.db.FBFriendRequest">

		<toolbar>
			<button name="new" title="New" hidden="true" />
			<button name="edit" title="Edit" hidden="true" />
			<button name="delete" title="Delete" />
			<button name="retriveRequest" title="Friend Request"
				onClick="com.axelor.sn.web.FBController:getFriendrequest" />
		</toolbar>
		<field name="name"></field>
		<field name="gender"></field>
		<field name="link"></field>
		<field name="curUser" hidden="true"></field>
	</grid>
	<form title="Detail View" name="fb-req-form" model="com.axelor.apps.base.db.FBFriendrequest">
		<field name="name" readonly="true"></field>
		<field name="gender" readonly="true"></field>
		<field name="link" readonly="true"></field>
	</form>
</object-views>
