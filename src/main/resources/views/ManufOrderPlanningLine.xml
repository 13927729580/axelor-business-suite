<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="manuf-order-planning-line-grid" title="MO planning lines" model="com.axelor.apps.production.db.ManufOrderPlanningLine">
        <field name="manufOrder"/>
        <field name="manufOrderPlanningLineSeq"/>
    </grid>
    
    <form name="manuf-order-planning-line-form" title="MO planning line" model="com.axelor.apps.production.db.ManufOrderPlanningLine" cols="4"
    onNew="action-manuf-order-planning-line-default-record">
        <field name="manufOrder"/>
        <field name="manufOrderPlanningLineSeq"/>
    </form>
    
    <action-record name="action-manuf-order-planning-line-default-record" model="com.axelor.apps.production.db.ManufOrderPlanningLine">
    	<field name="manufOrderPlanningLineSeq" expr="0"/>
    </action-record>
    
</object-views>
