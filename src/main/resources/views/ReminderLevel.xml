<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    
    <grid name="reminderLevel-grid" title="Reminder levels" model="com.axelor.apps.account.db.ReminderLevel">
        <field name="name"/>
        <field name="code"/>
        <field name="label"/>
    </grid>
    
    <form name="reminderLevel-form" title="Reminder level" model="com.axelor.apps.account.db.ReminderLevel" cols="6">
     	<panel title="Overview">
     		<field name="name"/>
    		<field name="code"/>
    		<field name="label"/>
     	</panel>
     	
    </form>
    
</object-views> 