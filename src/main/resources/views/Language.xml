<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    
    <grid name="language-grid" title="Languages" model="com.axelor.apps.base.db.Language" editable="true">
        <field name="name"/>
        <field name="code"/>
    </grid>
    
    <form name="language-form" title="Language" model="com.axelor.apps.base.db.Language" cols="4">
    	<panel>
	        <field name="name"/>
	        <field name="code"/>
        </panel>
    </form>
    
</object-views>