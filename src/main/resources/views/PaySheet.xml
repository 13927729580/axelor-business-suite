<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">

	<grid name="pay-sheet-grid" title="Pay sheets" model="com.axelor.apps.organisation.db.PaySheet">
		<field name="employee"/>
		<field name="period"/>
		<field name="paymentDate"/>
	</grid>

	<form name="pay-sheet-form" title="Pay sheet" model="com.axelor.apps.organisation.db.PaySheet">
		<panel>
			<field name="employee"/>
			<field name="period"/>
			<field name="paymentDate"/>
		</panel>
	</form>
	
	<grid name="pay-sheet-employee-grid" title="Pay sheets" model="com.axelor.apps.organisation.db.PaySheet">
		<field name="period"/>
		<field name="paymentDate"/>
	</grid>
	
	<form name="pay-sheet-employee-form" title="Pay sheet" model="com.axelor.apps.organisation.db.PaySheet">
		<panel>
			<field name="period"/>
			<field name="paymentDate"/>
		</panel>
	</form>
	
	
</object-views>
  