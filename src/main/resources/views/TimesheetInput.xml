	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="timesheet-input-grid" title="Timesheet inputs" model="com.axelor.apps.organisation.db.TimesheetInput">
        <field name="date"/>
        <field name="duration"/>
