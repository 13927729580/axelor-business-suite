<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    <grid name="formula-control-grid" title="Formula controls" model="com.axelor.apps.base.db.FormulaControl">
        <field name="constituent"/>
        <field name="pricingStructure"/>
        <field name="tax"/>
        <field name="taxLine"/>
        <field name="calculationRule"/>
        <field name="vatManagement"/>
        <field name="control"/>
        <field name="msg"/>
    </grid>
    <form name="formula-control-form" title="Formula control" model="com.axelor.apps.base.db.FormulaControl" cols="4">
        <field name="constituent"/>
        <field name="pricingStructure"/>
        <field name="tax"/>
        <field name="taxLine"/>
        <field name="calculationRule"/>
        <field name="vatManagement"/>
        <field name="control" colSpan="4"/>
        <field name="msg" colSpan="4"/>
    </form>
</object-views>