<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">

    <grid name="fiscal-position-grid" title="Fiscal positions" model="com.axelor.apps.account.db.FiscalPosition">
        <field name="name"/>
        <field name="code"/>
    </grid>

    <form name="fiscal-position-form" title="Fiscal position" model="com.axelor.apps.account.db.FiscalPosition" cols="4">
        <panel title="Overview">
	        <field name="name"/>
	        <field name="code"/>
	        <field name="accountEquivList" colSpan="4"/>
	        <field name="taxEquivList" colSpan="4"/>
        </panel>
     
    </form>

</object-views>