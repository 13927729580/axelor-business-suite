<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_3.0.xsd">

    <grid name="fiscal-position-grid" title="Fiscal positions" model="com.axelor.apps.account.db.FiscalPosition">
        <field name="name"/>
        <field name="code"/>
    </grid>

    <form name="fiscal-position-form" title="Fiscal position" model="com.axelor.apps.account.db.FiscalPosition"  >
        <panel>
	        <field name="name"/>
	        <field name="code"/>
	        <field name="accountEquivList"  />
	        <field name="taxEquivList"  />
        </panel>
    </form>

</object-views>