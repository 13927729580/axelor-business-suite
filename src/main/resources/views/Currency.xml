<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    
    <grid name="currency-grid" title="Currencies" model="com.axelor.apps.base.db.Currency">
        <field name="name"/>
        <field name="code"/>
        <field name="symbol"/>
    </grid>
    
    <form name="currency-form" title="Currency" model="com.axelor.apps.base.db.Currency">
    	<panel>
	        <field name="name"/>
	        <field name="code"/>
	        <field name="symbol"/>
        </panel>
    </form>
    
</object-views>