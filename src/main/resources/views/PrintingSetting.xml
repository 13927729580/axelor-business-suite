<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_3.0.xsd">
    
	<form name="printing-setting-form" title="Printing Setting" model="com.axelor.apps.base.db.PrintingSettings">
		<panel>
			<field name="languageSelect"/>
			<field name="defaultMailBirtTemplate" />
			<panel title="Company Header" colSpan="12">
				<field name="pdfHeader" colSpan="12" widget="html" showTitle="false"/>
			</panel>
			<panel title="Company Footer" colSpan="12">
	       		<field name="pdfFooter" colSpan="12" widget="html" showTitle="false"/>
	       	</panel>
       	</panel>
    </form>

</object-views>