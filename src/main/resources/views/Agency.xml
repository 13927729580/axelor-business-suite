<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    <grid name="agency-grid" title="Agences" model="com.axelor.apps.base.db.Agency">
        <field name="name"/>
        <field name="num"/>
        <field name="nameLong"/>
        <field name="nameShort"/>
        <field name="parentAgency"/>
        
    </grid>
    <form name="agency-form" title="Agency" model="com.axelor.apps.base.db.Agency" cols="4">
        <field name="name"/>
        <field name="code"/>
        <break/>
        <field name="nameLong"/>
        <field name="nameShort"/>
        <field name="num"/>
        <field name="parentAgency"/>
        <break/>
        <field name="address"/>
        <break/>
        <field name="openingHours" />
        <break/>
        <field name="userInfoList" colSpan="4"/>
    </form>
</object-views>
