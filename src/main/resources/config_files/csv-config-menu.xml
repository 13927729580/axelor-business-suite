<?xml version="1.0"?>
<csv-inputs xmlns="http://apps.axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/data-import http://apps.axelor.com/xml/ns/data-import/data-import_0.9.xsd">
	
	<input file="meta_menu.csv" type="com.axelor.meta.db.MetaMenu"
		separator="," search="self.name = :name" update="true">
		<bind to="name" column="name" />
		<bind to="groups" column="groups"  search="self.code in :groups" eval="groups.split('\\|') as List"/>
	</input>
	
</csv-inputs>