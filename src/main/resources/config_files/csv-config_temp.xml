<?xml version="1.0" encoding="UTF-8"?>
<csv-inputs xmlns="http://apps.axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/data-import http://apps.axelor.com/xml/ns/data-import/data-import_0.9.xsd">
  
	<input file="access_rights.csv" separator=";" type="com.axelor.meta.db.MetaMenu" search="self.name = :name" update="true">
		<bind column="groups" to="groups" search="self.code in :groups" eval="groups.split('\\|') as List"/>
	</input>
	
</csv-inputs>