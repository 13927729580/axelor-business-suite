<?xml version="1.0"?>
<csv-inputs xmlns="http://apps.axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/data-import http://apps.axelor.com/xml/ns/data-import/data-import_0.9.xsd">
	
	<input file="permission.csv" type="com.axelor.auth.db.Permission"
		separator="," search="self.name = :name">
		<bind to="name" column="name" />
		<bind to="object" column="object" />
		<bind to="canRead" eval="read == 'true'" />
		<bind to="canCreate" eval="create == 'true'" />
		<bind to="canRemove" eval="delete == 'true'" />
		<bind to="canWrite" eval="write == 'true'" />
		<bind to="canExport" eval="export == 'true'" />
	</input>
	
	<input file="permission.csv" type="com.axelor.auth.db.Permission" call="com.axelor.csv.script.ImportPermission:importPermission"
		separator="," search="self.name = :name">
		<bind to="name" column="name" />
	</input>
 	
</csv-inputs>
