<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">
	
  
  <grid name="ebics-partner-grid" title="Ebics partner" model="com.axelor.apps.bank.payment.db.EbicsPartner" >
		<field name="partnerId" />
	</grid>
	
	<form name="ebics-partner-form" title="Ebics partner" model="com.axelor.apps.bank.payment.db.EbicsPartner">
	
		<panel>
			<field name="partnerId" help="Name of the contract/subscription" max="35" x-bind="{{partnerId|uppercase}}" pattern='^[A-Z0-9]+$'/>
			<field name="ebicsBank" />
		</panel>
	
	</form>
	
</object-views>
