<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">
	
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="EbicsUser" lang="java">

	 <string name="securityMedium" title="Security medium" />
	 <string name="name" title="Name" namecolumn="true"/>
	 <string name="userId" title="User id" max="35"  />
	 <string name="dn" title="dn" unique="true" />
	 
	 <string name="password" title="Password" password="true" />
	 
	 <boolean name="isInitializedHIA" title="is initialized HIA ?"  />
	 <boolean name="isInitialized" title="is initialized ?"  />
	 
	 <binary name="a005PrivateKey" title="a005PrivateKey"  />
	 <binary name="a005Certificate" title="a005Certificate"  />
	 <binary name="a005PublicKey" title="a005PublicKey" />
	 
	 <binary name="e002PrivateKey" title="e002PrivateKey" />
	 <binary name="e002Certificate" title="e002Certificate" />
	 <binary name="e002PublicKey" title="e002PublicKey" />
	 
	 <binary name="x002PrivateKey" title="x002PrivateKey"/>
	 <binary name="x002Certificate" title="x002Certificate" />
	 <binary name="x002PublicKey" title="x002PublicKey"/>
	 
	 <string name="a005PublicKeyModulus" title="a005PublicKeyModulus" large="true" multiline="true" />
	 <string name="a005PublicKeyExponent" title="a005PublicKeyExponent" large="true" multiline="true" />
	 
	 <string name="e002PublicKeyModulus" title="e002PublicKeyModulus" large="true" multiline="true" />
	 <string name="e002PublicKeyExponent" title="e002PublicKeyExponent" large="true" multiline="true" />
	 
	 <string name="x002PublicKeyModulus" title="x002PublicKeyModulus" large="true" multiline="true" />
	 <string name="x002PublicKeyExponent" title="x002PublicKeyExponent" large="true" multiline="true" />
	 
	 <many-to-one name="ebicsPartner" ref="com.axelor.apps.account.db.EbicsPartner" title="Ebics partner"/>
	 
  </entity>
  
</domain-models>
