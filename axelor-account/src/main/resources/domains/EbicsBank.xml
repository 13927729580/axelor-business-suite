<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">
	
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="EbicsBank" lang="java">
  
	<string name="name" title="Name" namecolumn="true"/>
    <string name="url" title="Url" />
	<string name="hostId" title="Host id" />
	 
	<binary name="x002Key" title="Authentication Certificate Type X002"/>
	<binary name="x002Digest" title="Hash certificate of authentication (SHA-256)" />
	
	<binary name="e002Key" title="Encryption Certificate Type E002" />
	<binary name="e002Digest" title="Hash certificate of encryption (SHA-256)" />
	
	<string name="x002KeyModulus" title="X002 modulus" large="true" multiline="true" />
	<string name="x002KeyExponent" title="X002 exponent" large="true" multiline="true" />
	<string name="e002KeyModulus" title="E002 modulus" large="true" multiline="true" />
	<string name="e002KeyExponent" title="E002 exponent" large="true" multiline="true" />
	
  </entity>
  
</domain-models>
