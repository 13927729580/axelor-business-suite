<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
	
  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="EbicsBank" lang="java">
  
	<string name="name" title="Name" namecolumn="true"/>
    <string name="url" title="Url" />
	<string name="hostId" title="Host id" />
	
	<boolean name="testMode" title="Test model" />
	
	<many-to-one name="testFile" ref="com.axelor.meta.db.MetaFile" title="Test file" />
	
	<one-to-many name="ebicsCertificateList" ref="com.axelor.apps.account.db.EbicsCertificate" title="Certificates" mappedBy="ebicsBank" orphanRemoval="true"/>
	
  </entity>
  
</domain-models>
