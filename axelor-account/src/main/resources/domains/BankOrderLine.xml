<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="BankOrderLine" lang="java">
  
	<string name="sequence" title="Sequence" namecolumn="true" readonly="true" unique="true"/>  
  	<many-to-one name="bankOrder" ref="BankOrder" title="Bank Order"/>
  	<many-to-one name="receiverCompany" ref="com.axelor.apps.base.db.Company" title="Company"/>
	<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Partner"/>
	<many-to-one name="receiverBankDetails" ref="com.axelor.apps.base.db.BankDetails" title="Bank Account"/>
	<decimal name="amount" title="Amount"/>
	<string name="receiverReference" title="Reference" unique="true"/>
	<string name="receiverLabel" title="Label"/>
	<many-to-one name="receiverMove" ref="com.axelor.apps.account.db.Move" title="Move" readonly="true"/>
	
  	<boolean name="isRejected" title="Rejected"/>
  	<date name="rejectDate" title="Rejection date"/>
  	<many-to-one name="interbankCodeLine" ref="com.axelor.apps.account.db.InterbankCodeLine" title="Reject Reason"/>
  	<decimal name="rejectAmount" title="Amount Rejected"/>
  	<many-to-one name="rejectMove" ref="com.axelor.apps.account.db.Move" title="Reject Move" readonly="true"/>
  	
  </entity>

</domain-models>
