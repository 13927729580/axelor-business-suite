<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">
	
	
	<grid name="ebics-certificate-grid" title="Ebics certificate" model="com.axelor.apps.account.db.EbicsCertificate" >
		<field name="typeSelect" />
		<field name="subject" />
		<field name="issuer" />
		<field name="validFrom" />
		<field name="validTo" />
	</grid>
	
	<form name="ebics-certificate-form" title="Ebics certificate" model="com.axelor.apps.account.db.EbicsCertificate">
		<panel>
			<field name="typeSelect" />
			<field name="subject" colSpan="12" />
			<field name="issuer" colSpan="12" />
			<field name="validFrom" />
			<field name="validTo" />
			<field name="certificate" />
		</panel>
	</form>
  
</object-views>
