<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">
	
	
	<grid name="ebics-bank-grid" title="Ebics bank" model="com.axelor.apps.account.db.EbicsBank" >
		<field name="name" />
		<field name="url" />
		<field name="hostId" />
	</grid>
	
	<form name="ebics-bank-form" title="Ebics bank" model="com.axelor.apps.account.db.EbicsBank">
	
		<panel title="General informations">
			<field name="name"/>
			<field name="url"/>
			<field name="hostId" help="Identifies the bank" />
			<field name="testMode" />
			<field name="testFile" widget="binary-link" showIf="testMode"/>
		</panel>

		<panel-related field="ebicsCertificateList" form-view="ebics-certificate-form" grid-view="ebics-certificate-grid" />
	</form>
	
  
</object-views>
