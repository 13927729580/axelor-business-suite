<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">
	
	
	<grid name="ebics-bank-grid" title="Ebics bank" model="com.axelor.apps.account.db.EbicsBank" >
		<field name="name" />
		<field name="url" />
		<field name="hostId" />
	</grid>
	
	<form name="ebics-bank-form" title="Ebics bank" model="com.axelor.apps.account.db.EbicsBank">
	
		<panel title="General informations">
			<field name="name" />
			<field name="url" />
			<field name="hostId" help="Identifies the bank" />
		</panel>
		<panel title="Server public key certificate dedicated to authentication" itemSpan="12">
			<field name="e002Key" />
			<field name="e002Digest" />
		</panel>
		<panel title="Server public key certificate dedicated to encryption" itemSpan="12">
			<field name="x002Key" />
			<field name="x002Digest" />
		</panel>
	
	</form>
  
  
	
  
</object-views>
