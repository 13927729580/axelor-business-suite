<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="base" package="com.axelor.apps.base.db"/>

  	<entity name="DocumentGenerator" lang="java">
		<many-to-one name="docInputFile" ref="com.axelor.meta.db.MetaFile" title="Input File" required="true" />
		<string name="exportType" title="Export Type" default="html" selection="document.export.type.select" required="true"/>
		<many-to-one name="docOutputFile" ref="com.axelor.meta.db.MetaFile" title="Output File"/>
  	</entity>

</domain-models>
