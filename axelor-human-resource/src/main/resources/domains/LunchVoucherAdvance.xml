<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="human-resource" package="com.axelor.apps.hr.db"/>

  <entity name="LunchVoucherAdvance" cachable="true">
      
      <date name="distributionDate" title="Distribution date"/>
      <integer name="nbrLunchVouchers" title="Lunch vouchers distributed" />
      <decimal name="wageShare" title="Wage Share" scale="2" precision="20" readonly="true"/>
      <decimal name="employersShare" title="Employer's Share" scale="2" precision="20" readonly="true"/>
      <string name="comments" title="Comment" large="true"/>
  </entity>
  
</domain-models>
