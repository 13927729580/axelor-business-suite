<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">

  <module name="human-resource" package="com.axelor.apps.human.resource.db"/>

  <entity name="Timesheet" cachable="true">
      
      <date name="sentDate" title="Sent Date" />
      <integer name="statusSelect" title="Status" selection="human.resources.timesheet.status.select" default="1"/>
      <many-to-one name="user" ref="com.axelor.auth.db.User" title="User" column="user_id" required="true"/>
      <one-to-many name="timesheetLineList" ref="com.axelor.apps.human.resource.db.TimesheetLine" title="Log time"/>
      
  </entity>
  
</domain-models>
