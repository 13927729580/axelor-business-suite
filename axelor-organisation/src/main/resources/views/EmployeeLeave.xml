<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_3.0.xsd">

	<grid name="employee-leave-grid" title="Employee leaves" model="com.axelor.apps.organisation.db.EmployeeLeave">
		<field name="reasonTask"/>
		<field name="leaveBalances"/>
		<field name="leaveRequestInProgress"/>
		<field name="cumulation"/>
	</grid>

	<form name="employee-leave-form" title="Employee leave"	model="com.axelor.apps.organisation.db.EmployeeLeave">
		<panel>
			<field name="reasonTask"/>
			<field name="leaveBalances"/>
			<field name="leaveRequestInProgress"/>
			<field name="cumulation"/>
		</panel>
	</form>
	
</object-views>
  