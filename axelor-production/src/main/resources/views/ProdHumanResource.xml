	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">
    
    <grid name="prod-human-resource-grid" title="Prod. Human resources" model="com.axelor.apps.production.db.ProdHumanResource">
        <field name="product"/>
        <field name="employee"/>
        <field name="duration" widget="duration" x-big="true" x-seconds="true"/>
    </grid>
    
    <form name="prod-human-resource-form" title="Prod. Human resource" model="com.axelor.apps.production.db.ProdHumanResource">
		<panel>
	        <field name="product"/>
	        <field name="employee"/>
	        <field name="duration" widget="duration" x-big="true" x-seconds="true"/>
        </panel>
    </form>
    
</object-views>
