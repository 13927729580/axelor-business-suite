<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="stock" package="com.axelor.apps.stock.db"/>

  <entity name="StockConfig" lang="java" cachable="true">
      <many-to-one name="productionVirtualLocation" ref="com.axelor.apps.stock.db.Location" title="Production virtual location"/>
      <many-to-one name="wasteLocation" ref="com.axelor.apps.stock.db.Location" title="Waste location"/>
      <many-to-one name="componentLocation" ref="com.axelor.apps.stock.db.Location" title="Components location"/>
      <many-to-one name="finishedProductsLocation" ref="com.axelor.apps.stock.db.Location" title="Finished products location"/>
  </entity>

</domain-models>