<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="base" package="com.axelor.apps.stock.db"/>

  <entity name="PartnerProductQualityRate" lang="java" cachable="true">
    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner"/>
    <many-to-one name="product" ref="com.axelor.apps.base.db.Product"/>
    <decimal name="qualityRate" title="Quality rate"/>
    <decimal name="partnerProductMoveLineTotal" title="Product Move Line Total"/>
    <decimal name="supplierRate" selection="partner.product.quality.rate.supplier.percent.rate.select"/>
	<finder-method name="findProductByName" using="product, partner"/>

  </entity>
  
</domain-models>
    