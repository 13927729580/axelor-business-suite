<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
	<module name="googleapps" package="com.axelor.apps.googleapps.db" />
	<entity name="GoogleTask">
		<string name="taskId" title="Task Id"/>
		<string name="taskTitle" title="Task Title" required="true"/>
		<string name="taskNote" title="Note" multiline="true" large="true"/>
		<datetime name="taskDue" title="Due Date" />
		<string name="status" title="status" />
		<datetime name="completedDate" title="Completed Date"/>
		<many-to-one name="user" column="user_id" ref="com.axelor.apps.base.db.UserInfo" title="User"/>
	</entity>
</domain-models>
