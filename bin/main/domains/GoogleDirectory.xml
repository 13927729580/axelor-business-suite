<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">
		<module name="googleapps" package="com.axelor.apps.googleapps.db" />
			<entity name="GoogleDirectory">
				<string name="directoryId" title="Directory Id" help="Google Directory Id" />
				<string name="directoryName" title="directory Name" namecolumn="true"  help="Directory Name"/> 
				<boolean name="trashed" title="Trashed" default="false" help="Check file is Trashed or not" />
				<many-to-one name="parent" ref="com.axelor.apps.googleapps.db.GoogleDirectory" title="Parent Directory" help="Parent Directory name" />
				<many-to-one name="driveUser" ref="com.axelor.apps.base.db.UserInfo" orphan="true" help="Google Apps User" />
				<one-to-many name="googleFiles" ref="GoogleFile" mappedBy="googleDirectory" orphan="true"  help="Google Files in Directory"/>	
				
	      </entity>
</domain-models>
