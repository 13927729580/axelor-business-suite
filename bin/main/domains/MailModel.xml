<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="MailModel" lang="java">
	
	<integer name="importId"/>
    <string name="code" title="Code" required="true"/>
    <string name="name" title="Label" required="true"/>
    <integer name="typeSelect" title="Type" selection="mail.type.select"/>
    <integer name="objectConcernedSelect" title="Element concerned" selection="mail.model.object.concerned.select"/>
    <string name="reminderTypeSelect" title="Reminder type" selection="mail.model.reminder.type.select"/>
    <string name="subject" title="Subject" required="true"/>
    <string name="content" title="Content" multiline="true"/>
    <string name="pdfModelPath" title="Pdf Birt model path"/>

  </entity>

</domain-models>
