    <form name="canton-form" title="Canton" model="com.axelor.apps.base.db.Canton" cols="4">
        <field name="name"/>
        <field name="code"/>
        <field name="artmin"/>
        <field name="department"/>
    	<field name="inhabitantsNbr" />
    </form>
</object-views>