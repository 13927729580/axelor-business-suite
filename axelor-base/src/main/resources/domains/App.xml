<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">

	<module name="base" package="com.axelor.apps.base.db"/>

  	<entity name="App" lang="java">
		
		<string name="typeSelect" title="Type" selection="app.type.select"  required="true" unique="true"/>
	    <string name="name" title="Name" required="true" translatable="true"/>
	    <string name="code" title="Code"/>
	    <string name="description" title="Description" large="true" translatable="true"/>
		<many-to-one name="baseGeneral" ref="com.axelor.apps.base.db.General" title="General configuration" />
		<many-to-one name="image" ref="com.axelor.meta.db.MetaFile" title="Image"/>
		<string name="dependsOn" title="Depends on" selection="app.type.select"/>
		<boolean name="initDataLoaded" title="Init data loaded ?" />
		<boolean name="demoDataLoaded" title="Demo data loaded ?" />
		<string name="modules" title="Modules contains in the app" required="true" />
	    
  	</entity>

</domain-models>
