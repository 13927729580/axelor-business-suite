<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	
	<grid name="project-folder-grid" title="Projects Folder" model="com.axelor.apps.project.db.ProjectFolder">
		<field name="name"/>
		<field name="description"/>
		<field name="sequence"/>
	</grid>
	
	<form title="Projects Folder" name="project-folder-form"  model="com.axelor.apps.project.db.ProjectFolder">
		<panel>
			<field name="sequence" readonly="true"/>
			<spacer/>
			<field name="name"/>
			<spacer/>
			<field name="description" colSpan="12"/>
			<field name="membersUserSet" colSpan="12"/>
			<field name="projectList" colSpan="12"/>
		</panel>
		
	</form>
	
</object-views>